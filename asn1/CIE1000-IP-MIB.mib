    cie1000IpStatisticsInterfacesIpv6OutNoRoutes         Unsigned32,
    cie1000IpStatisticsInterfacesIpv6OutForwDatagrams    Unsigned32,
    cie1000IpStatisticsInterfacesIpv6HCOutForwDatagrams  Counter64,
    cie1000IpStatisticsInterfacesIpv6OutDiscards         Unsigned32,
    cie1000IpStatisticsInterfacesIpv6OutFragReqds        Unsigned32,
    cie1000IpStatisticsInterfacesIpv6OutFragOKs          Unsigned32,
    cie1000IpStatisticsInterfacesIpv6OutFragFails        Unsigned32,
    cie1000IpStatisticsInterfacesIpv6OutFragCreates      Unsigned32,
    cie1000IpStatisticsInterfacesIpv6OutTransmits        Unsigned32,
    cie1000IpStatisticsInterfacesIpv6HCOutTransmits      Counter64,
    cie1000IpStatisticsInterfacesIpv6OutOctets           Unsigned32,
