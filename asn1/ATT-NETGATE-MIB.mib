      custSecureID(2),
      custAxent(3),
      custCert(4),
      custLDAP(5),
      custInternal(6),
      branchSharedSecret(7),
      agnsManaged(8)
    }
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
