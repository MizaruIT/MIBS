FORTIOS-300-MIB DEFINITIONS ::= BEGIN
	IMPORTS
		TEXTUAL-CONVENTION, DisplayString
			FROM SNMPv2-TC
		ifIndex, ifEntry
			FROM IF-MIB
		MODULE-IDENTITY, OBJECT-TYPE, NOTIFICATION-TYPE, enterprises,
		Integer32, Gauge32, IpAddress, Counter32, Counter64, TimeTicks
			FROM SNMPv2-SMI
		sysName
			FROM SNMPv2-MIB
		InetPortNumber
			FROM INET-ADDRESS-MIB
		OBJECT-GROUP, NOTIFICATION-GROUP, MODULE-COMPLIANCE
			FROM SNMPv2-CONF;

	fortinet MODULE-IDENTITY
		LAST-UPDATED "200806190000Z" -- Jun. 19, 2008
		ORGANIZATION "Fortinet Technologies, Inc."
		CONTACT-INFO
			"
			      Technical Support
			      email: support@fortinet.com
			      http://www.fortinet.com
			"
		DESCRIPTION
			"MIB module for Fortinet network devices."

		REVISION "200806190000Z" -- Jun. 19, 2008
		DESCRIPTION "Renamed to less ambiguous FORTIOS-300-MIB."

		REVISION "200806160000Z" -- Jun. 16, 2008
		DESCRIPTION "Spelling corrections.
			Added fnVpnDialupInOctets and fnVpnDialupOutOctets."

		REVISION "200804140000Z" -- Apr. 14, 2008
		DESCRIPTION "Added fnTrapPowerSupplyFailure (106) trap."

		REVISION "200804090000Z" -- Apr. 9, 2008
		DESCRIPTION "Added fnTrapTest (999) trap.
			Added new platforms: FG-30B, FG-5001A, FG-310B."
		
		REVISION "200801100000Z" -- Jan. 10, 2008
		DESCRIPTION "Improved descriptions."
		
		REVISION "200801020000Z" -- Jan. 2, 2008
		DESCRIPTION "Added spam counts to SMTP, POP3, and IMAP stats."
		
		REVISION "200712050000Z" -- Dec. 5, 2007
		DESCRIPTION "Corrected description of fnHaStatsNetUsage to Kbps.
			Added new per-vdom/per-protocol P2P statistics table."

		REVISION "200711270000Z" -- Nov. 27, 2007
		DESCRIPTION "Added fnIntfEntry as an augmentation of MIB-2 ifEntry.
			Added vdom indexes to admin and user tables.
			Removed unused OIDs.
			Updated ACCESS for some trap parameters."

		REVISION "200711050000Z" -- Nov. 5, 2007
		DESCRIPTION "Removed unused OID for per-vdom session count.
			Added missing model identifiers."

		REVISION "200709120000Z" -- Sept. 12, 2007
		DESCRIPTION "Refreshed for release, file formatting changes."

		REVISION "200708090000Z" -- Aug. 9, 2007
		DESCRIPTION "New statistics tables for applications such
			as proxies, VoIP and P2P. New stats for per vdom
			session counter and per policy byte counter."

		REVISION "200706060000Z" -- Jun. 6, 2007
		DESCRIPTION "New traps, model definitions, and 
			extended vdom specific information for proxies
			and anti-virus.
			File formatting and cleanup.
			Intended for use with FortiOS 3.00 MR5 and newer."

		REVISION "200611230000Z" -- Nov. 23, 2006
		DESCRIPTION "Added new trap for internal use.
			Trap is to indicate completion of config
			deployment to the managing FortiManager."

		REVISION "200608030000Z" -- Aug. 3, 2006
		DESCRIPTION "Added new OIDs for non-dial-up VPN tunnels.
			Added new table and traps for hardware sensors."

		REVISION "200607310000Z" -- Jul. 31, 2006
		DESCRIPTION "Added new HA trap for heartbeat failure.
			Changed variables to use InetPortNumber as type,
			where appropriate.
			Added value range information to % fields.
			Added 1000A-LENC platform id."

		REVISION "200607280000Z" -- Jul. 28, 2006
		DESCRIPTION "Added new platforms to platform list."

		REVISION "200606210000Z" -- Jun. 21, 2006
		DESCRIPTION "Expanded traps to include sysName (hostname).
			Added new traps for other AV events, including
			oversize file blocking, file pattern matching, and
			fragmented file blocking.
			Renamed main AV trap to show actual usage."

		REVISION "200604260000Z" -- Apr. 26, 2006
		DESCRIPTION "Expanded vpn up/down traps and added
			OID's for local and remote gateway parameters.
			Updated Wifi platforms."

		REVISION "200601090000Z" -- Jan. 9, 2006
		DESCRIPTION "Added OID's for hdd usage and capacity.
			Added 5004 platform."

		REVISION "200512070000Z" -- Dec. 7, 2005
		DESCRIPTION "Added missing OID's for several traps used in
			communication between FortiGate and FortiManager.
			Also added some missing models.
			Moved fnIps group to correct OID."

		REVISION "200509200000Z" -- Sept. 20, 2005
		DESCRIPTION "Initial creation of unified MIB for products
			based on FortiOS >=3.00"

		::= { enterprises 12356 }

	--
	-- Textual conventions
	--

	FnBoolState ::= TEXTUAL-CONVENTION
		STATUS current
		DESCRIPTION "Boolean data type representing enabled/disabled"
		SYNTAX INTEGER {
			disabled ( 1 ),
			enabled  ( 2 )
		}

	FnIndex ::= TEXTUAL-CONVENTION
		DISPLAY-HINT "d"
		STATUS current
		DESCRIPTION "data type for table index values"
		SYNTAX Integer32 ( 0 .. 2147483647 )

	FnVdIndex ::= TEXTUAL-CONVENTION
		DISPLAY-HINT "d"
		STATUS current
		DESCRIPTION "data type for virtual-domain indexes"
		SYNTAX Integer32 ( 1 .. 2147483647 )

	FnOpMode ::= TEXTUAL-CONVENTION
		STATUS current
		DESCRIPTION "enumerated type for device operation modes"
		SYNTAX INTEGER {
			nat         ( 1 ),
			transparent ( 2 )
		}

	FnHaMode ::= TEXTUAL-CONVENTION
		STATUS current
		DESCRIPTION "enumerated type for HA cluster modes"
		SYNTAX INTEGER {
			standalone     ( 1 ),
			activeActive   ( 2 ),
			activePassive  ( 3 )
		}

	FnHaLBSchedule ::= TEXTUAL-CONVENTION
		STATUS current
		DESCRIPTION "enumerated type for HA load-balancing schedules"
		SYNTAX INTEGER {
			none                 ( 1 ),
			hub                  ( 2 ),
			leastConnections     ( 3 ),
			roundRobin           ( 4 ),
			weightedRoundRobin   ( 5 ),
			random               ( 6 ),
			ipBased              ( 7 ),
			ipPortBased          ( 8 )
		}

	FnAdminPermLevel ::= TEXTUAL-CONVENTION
		STATUS current
		DESCRIPTION "enumerated type for administrative permissions"
		SYNTAX INTEGER {
			readAdmin   ( 0 ),
