--
-- NOTIFICATIONS-TYPES / traps
--
--

--
-- fortinet.fnTraps.
--

	fnTrapObjects          OBJECT IDENTIFIER ::= { fnTraps 1 }

-- Sent from FortiOS to FortiManager only
	fnFMTrapIfChange        NOTIFICATION-TYPE
		OBJECTS         { fnManSysSerial, fnManIfName, fnManIfIp, fnManIfMask }
		STATUS          current
		DESCRIPTION     "Trap is sent to the managing FortiManager if an interface IP is changed"
		::=  { fnTraps  100  }

	fnTrapCpuHigh           NOTIFICATION-TYPE
		OBJECTS         { fnSysSerial, sysName }
		STATUS          current
		DESCRIPTION     "Indicates that the CPU usage has exceeded the configured threshold"
		::=  { fnTraps  101  }

	fnTrapMemLow            NOTIFICATION-TYPE
		OBJECTS         { fnSysSerial, sysName }
		STATUS          current
		DESCRIPTION     "Indicates memory usage has exceeded the configured threshold"
		::=  { fnTraps  102  }

	fnTrapLogFull           NOTIFICATION-TYPE
		OBJECTS         { fnSysSerial, sysName }
		STATUS          current
		DESCRIPTION     "Log disk usage has exceeded the configured threshold"
		::=  { fnTraps  103  }

	fnTrapTempHigh          NOTIFICATION-TYPE
		OBJECTS         { fnSysSerial, sysName }
		STATUS          current
		DESCRIPTION     "A temperature sensor on the device has exceeded its threshold. Not available on all models."
		::=  { fnTraps  104  }

	fnTrapVoltageOutOfRange NOTIFICATION-TYPE
		OBJECTS         { fnSysSerial, sysName }
		STATUS          current
