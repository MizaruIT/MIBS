-- %CLISHOWGROUP inventory.1
iShelfCardInventoryFlashSoftwareRevision OBJECT-TYPE
    SYNTAX      IShelfCardSoftwareRevision
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "This attribute indicates the flash software revision of the card."
    ::= { iShelfCardInventoryEntry 6 }

-- %CLICOLLABEL Date
-- %CLISHOWGROUP inventory.2
iShelfCardInventoryDate OBJECT-TYPE
    SYNTAX      IShelfDateTime
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "This attribute indicates the release date of the
         firmware for the card."
    ::= { iShelfCardInventoryEntry 7 }

-- %CLICOLLABEL Serial
-- %CLISHOWGROUP inventory.2
iShelfCardInventorySerialNumber OBJECT-TYPE
    SYNTAX      IShelfSerialNumber
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "This attribute indicates the serial number of the card."
    ::= { iShelfCardInventoryEntry 8 }

-- %CLICOLLABEL Fpga
-- %CLISHOWGROUP inventory.2
iShelfCardInventoryFpga OBJECT-TYPE
    SYNTAX      IShelfFpga
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "This attribute indicates the revision number of the
         Field-Programmable Gate Array (FPGA) logic chip."
    ::= { iShelfCardInventoryEntry 9 }

-- %CLICOLLABEL CLEI
-- %CLISHOWGROUP inventory.2
iShelfCardInventoryCleiCode OBJECT-TYPE
    SYNTAX      IShelfCleiCode
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "This attribute indicates the CLEI code of the card."
    ::= { iShelfCardInventoryEntry 10 }

-- %CLICOLLABEL ProdId
-- %CLISHOWGROUP inventory.2
iShelfCardInventoryProductId OBJECT-TYPE
    SYNTAX      IShelfProductId
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "This attribute indicates the product ID of the card."
    ::= { iShelfCardInventoryEntry 11 }

-- %CLICOLLABEL Fpga2
-- %CLISHOWGROUP inventory.2
iShelfCardInventoryFpga2 OBJECT-TYPE
    SYNTAX      IShelfFpga
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "This attribute indicates the revision number of the
         2nd Field-Programmable Gate Array (FPGA) logic chip if the card
         is so equipped."
    ::= { iShelfCardInventoryEntry 12 }

