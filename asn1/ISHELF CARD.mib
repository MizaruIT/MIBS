-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
--
-- iShelfOltPonTable -
--
-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++

-- %DEEPGET CUSTOM 0 ROW
-- %CLICMDPREFIX card
-- %CLICMDABBR oltpon
-- %CLISHOWGROUP oltpon
iShelfOltPonTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IShelfOltPonEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This table contains the provisioning for the OLT PON interface. Provisioning
         of the PON at this level is basically turning the transmit laser on or off.
         The table also contains various PON-level statistics."
    ::= { iShelfOltCard 1 }

iShelfOltPonEntry OBJECT-TYPE
    SYNTAX      IShelfOltPonEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "Each row controls a separate PON interface."
    INDEX       { iShelfOltPonLocation,
                  iShelfOltPonIndex }
    ::= { iShelfOltPonTable 1 }

IShelfOltPonEntry ::=
    SEQUENCE {
        iShelfOltPonLocation
            IShelfLocation,
        iShelfOltPonIndex
            IShelfOltPonIndex,
        iShelfOltPonAdminState
            IShelfAdminState,
        iShelfOltPonOperState
            IShelfOperState,
        iShelfOltPonUsTotalAal5Bandwidth
            IShelfOltPonBandwidthRate,
        iShelfOltPonDsTotalAal5Bandwidth
            IShelfOltPonBandwidthRate,
        iShelfOltPonUsAvailAal5Bandwidth
            IShelfOltPonBandwidthRate,
        iShelfOltPonDsAvailAal5Bandwidth
            IShelfOltPonBandwidthRate,
        iShelfOltPonUsUsedAal12Bandwidth
            IShelfOltPonBandwidthRate,
        iShelfOltPonDsUsedAal12Bandwidth
            IShelfOltPonBandwidthRate,
        iShelfOltPonInCells
            Counter32,
        iShelfOltPonInHecErrors
            Counter32,
        iShelfOltPonInLengthErrors
            Counter32,
        iShelfOltPonInPayloadTypeErrors
            Counter32,
        iShelfOltPonInInvalidVCErrors
            Counter32,
        iShelfOltPonOutCells
            Counter32,
        iShelfOltPonRowStatus
            RowStatus
    }

-- %CLICOLLABEL loc
-- %CLISHOWGROUP oltpon.1
-- %CLISHOWGROUP oltpon.2
-- %CLISHOWGROUP oltpon.3
-- %CLIWIDTH 6
iShelfOltPonLocation OBJECT-TYPE
    SYNTAX      IShelfLocation
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "The PON interface resides on the OLT card at this location."
    ::= { iShelfOltPonEntry 1 }

-- %CLICOLLABEL pon
-- %CLISHOWGROUP oltpon.1
-- %CLISHOWGROUP oltpon.2
-- %CLISHOWGROUP oltpon.3
-- %CLIWIDTH 3
iShelfOltPonIndex OBJECT-TYPE
    SYNTAX      IShelfOltPonIndex
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The PON index on the OLT card. There are 2 types of OLT cards:
         a two PON and a four PON version."
    ::= { iShelfOltPonEntry 2 }

-- %CLICOLLABEL admin
-- %CLISHOWGROUP oltpon.1
iShelfOltPonAdminState OBJECT-TYPE
    SYNTAX      IShelfAdminState
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "The provisioned administrative state of the PON. Note that in
         the locked state, the transmit laser is off. Likewise, in the
         unlocked state, the transmit laser is on."
    DEFVAL      { lock }
    ::= { iShelfOltPonEntry 3 }

-- for DeepGet() 
-- %CLICOLLABEL oper
-- %CLISHOWGROUP oltpon.1
iShelfOltPonOperState OBJECT-TYPE
    SYNTAX      IShelfOperState
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "The current operational state of the PON."
    ::= { iShelfOltPonEntry 4 }

-- %CLICOLLABEL usTotAAL5
-- %CLISHOWGROUP oltpon.2
iShelfOltPonUsTotalAal5Bandwidth OBJECT-TYPE
    SYNTAX      IShelfOltPonBandwidthRate
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "This object identifies the total available AAL5
         bandwidth upstream for this PON at system initialization.
         The manager computes used AAL5 bandwidth upstream as
         follows:  UsUsedAal5 = UsTotalAal5 - UsAvailAal5 - UsUsedAal12."
    ::= { iShelfOltPonEntry 5 }

-- %CLICOLLABEL dsTotAAL5
-- %CLISHOWGROUP oltpon.2
iShelfOltPonDsTotalAal5Bandwidth OBJECT-TYPE
    SYNTAX      IShelfOltPonBandwidthRate
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "This object identifies the total available AAL5
         bandwidth downstream for this PON at system initialization.
         The manager computes used AAL5 bandwidth downstream as
         follows:  DsUsedAal5 = DsTotlaAal5 - DsAvailAal5 - DsUsedAal12."
    ::= { iShelfOltPonEntry 6 }

-- %CLICOLLABEL usAvAAL5
-- %CLISHOWGROUP oltpon.2
iShelfOltPonUsAvailAal5Bandwidth OBJECT-TYPE
    SYNTAX      IShelfOltPonBandwidthRate
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "This object identifies the currently available AAL5
         bandwidth upstream for this PON.  This object is maintained by
         the OLT as bandwidth is managed.  It may be used for auditing
         purposes by the manager.

         AAL5 traffic is used for data and SDV services."
    ::= { iShelfOltPonEntry 7 }

-- %CLICOLLABEL dsAvAAL5
-- %CLISHOWGROUP oltpon.2
iShelfOltPonDsAvailAal5Bandwidth OBJECT-TYPE
    SYNTAX      IShelfOltPonBandwidthRate
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "This object identifies the currently available AAL5
         bandwidth downstream for this PON.  This object is maintained
         by the OLT as bandwidth is managed.  It may be used for auditing
         purposes by the manager.

         AAL5 traffic is used for data and SDV services."
    ::= { iShelfOltPonEntry 8 }

-- %CLICOLLABEL usUsedAAL5
-- %CLISHOWGROUP oltpon.2
iShelfOltPonUsUsedAal12Bandwidth OBJECT-TYPE
    SYNTAX      IShelfOltPonBandwidthRate
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "This object identifies the currently used AAL1/2
         bandwidth upstream for this PON.  This object is maintained
         by the OLT as bandwidth is managed.  It may be used for
         auditing purposes by the manager.

         AAL1/2 traffic is used for T1 and POTS services, respectively."
    ::= { iShelfOltPonEntry 9 }

-- %CLICOLLABEL dsUsedAAL5
-- %CLISHOWGROUP oltpon.2
iShelfOltPonDsUsedAal12Bandwidth OBJECT-TYPE
    SYNTAX      IShelfOltPonBandwidthRate
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "This object identifies the currently used AAL1/2
         bandwidth downstream for this PON.  This object is maintained
         by the OLT as bandwidth is managed.  It may be used for
         auditing purposes by the manager.

         AAL1/2 traffic is used for T1 and POTS services, respectively."
    ::= { iShelfOltPonEntry 10 }

