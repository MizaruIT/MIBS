-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
--
-- iShelfOntSubscriberLineCardTable -
--
-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++

-- %CLICMDPREFIX card
-- %CLICMDABBR ontslctable
-- %CLISHOWGROUP ontslctable
iShelfOntSubscriberLineCardTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IShelfOntSubscriberLineCardEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This table represents the slots of the ONT that are capable
         of holding subscriber line cards.  A row in this table exists for
         each slot.

         Rows in this table are automatically created when an ONT is provisioned.

         The number of slots for an ONT is determiend by the ONT type.  Rows in
         this table are alos created for ONTs with integrated interfaces.
         Therefore, both integrate and expansion slots are supported by this table.

         Global provisioning of the subscriber line card ports is also supported
         by this table.

         For expansion slots, the management application is able to provision
         the expected type.  Pre-provisioning of expansion slots is supported.
         When the actual expansion card is plugged in, a check will be made to
         verify the correct type.  If the type is correct, the provisioning
         will automatically be applied to the ONT subscriber line card."
    ::= { iShelfOntCard 6 }

iShelfOntSubscriberLineCardEntry OBJECT-TYPE
    SYNTAX      IShelfOntSubscriberLineCardEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each entry contains the global information for one ONT
         subscriber line card."
    INDEX       { iShelfOntSubscriberLineCardLogicalId,
                  iShelfOntSubscriberLineCardIndex }
    ::= { iShelfOntSubscriberLineCardTable 1 }

IShelfOntSubscriberLineCardEntry ::=
    SEQUENCE {
        iShelfOntSubscriberLineCardLogicalId
            IShelfOntLogicalId,
        iShelfOntSubscriberLineCardIndex
            IShelfOntSubscriberLineCardIndex,
        iShelfOntSubscriberLineCardExpectedType
            IShelfOntSubscriberLineCardType,
        iShelfOntSubscriberLineCardActualType
            IShelfOntSubscriberLineCardType,
        iShelfOntSubscriberLineCardExpectedPortCount
            INTEGER,
        iShelfOntSubscriberLineCardAdminState
            IShelfAdminState,
        iShelfOntSubscriberLineCardOperState
            IShelfOperState,
        iShelfOntSubscriberLineCardRowStatus
            RowStatus
    }

-- %CLISHOWGROUP ontslctable
iShelfOntSubscriberLineCardLogicalId OBJECT-TYPE
    SYNTAX      IShelfOntLogicalId
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "This object identifies the logical ID of the ONT."
    ::= { iShelfOntSubscriberLineCardEntry 1 }

-- %CLISHOWGROUP ontslctable
iShelfOntSubscriberLineCardIndex OBJECT-TYPE
    SYNTAX      IShelfOntSubscriberLineCardIndex
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "This object identifies the subscriber line card slot."
    ::= { iShelfOntSubscriberLineCardEntry 2 }

-- %CLISHOWGROUP ontslctable
-- %CLICOLLABEL expType
iShelfOntSubscriberLineCardExpectedType OBJECT-TYPE
    SYNTAX      IShelfOntSubscriberLineCardType
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This object identifies the expected type of the
         subscriber line card.  Note that this value is only writeable
         for non-integrated line cards.  For integrated line card slots
         this value is automatically initialized according to the actual
         type.  For non-integrated slots this value is initialized to
         a value of unknown.

         NOTE:  This object should actually be read-create however
         this causes validation issues with some tools with the row status
         as other objects in the table are truely read-write.  This object
         is not setable after creation but must be deleted and recreated."
    ::= { iShelfOntSubscriberLineCardEntry 3 }

-- %CLISHOWGROUP ontslctable
-- %CLICOLLABEL actType
iShelfOntSubscriberLineCardActualType OBJECT-TYPE
    SYNTAX      IShelfOntSubscriberLineCardType
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "This object identifies the actual plug-in type of the
         subscriber line card."
    DEFVAL      { empty }
    ::= { iShelfOntSubscriberLineCardEntry 4 }

-- %CLISHOWGROUP ontslctable
-- %CLICOLLABEL numPort
iShelfOntSubscriberLineCardExpectedPortCount OBJECT-TYPE
    SYNTAX      INTEGER (1..8)
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "This object identifies the number of expected subscriber
         line card ports for this slot.  For non-integrated slots that are
         empty, this value will be zero."
    ::= { iShelfOntSubscriberLineCardEntry 5 }

-- %CLISHOWGROUP ontslctable
iShelfOntSubscriberLineCardAdminState OBJECT-TYPE
    SYNTAX      IShelfAdminState
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This object is used to unlock and lock the services for
         this subscriber line card."
    DEFVAL      { unlock }
    ::= { iShelfOntSubscriberLineCardEntry 6 }

-- %CLISHOWGROUP ontslctable
iShelfOntSubscriberLineCardOperState OBJECT-TYPE
    SYNTAX      IShelfOperState
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "This object indicates whether or not this subscriber
         line card is capable of performing its services.  Valid
         values are enabled, disabled, and unknown.  A value of disabled
         indicates the line card is present, but for some reason it is
         not capable of performing its service, e.g., actual vs. expected
         line card type mismatch.  A value of unknown indicates the line
         card is not present."
    DEFVAL      { unknown }
    ::= { iShelfOntSubscriberLineCardEntry 7 }

-- %CLISHOWGROUP ontslctable
-- %CLICOLLABEL RowStatus
-- %ROWSTATUSTYPE RFC
iShelfOntSubscriberLineCardRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This object is used for row creation, modification and
         deletion. Modification of objects within the row is only allowed
         when the row is not in the active state. In the case of an active
         row (already created), the row must be made notInService prior to
         modifying any settable objects. The notInService setting has no
         impact on the underlying object but is used to cause atomic updates
         of the row once it is moved to the active state.

         The supported values of RowStatus are:
            active (1)
            notInService (2)
            notReady (3)
            createAndGo (4)
            createAndWait (5)
            destroy (6)"
    ::= { iShelfOntSubscriberLineCardEntry 8 }

