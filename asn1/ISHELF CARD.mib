-- iShelfCardInventoryTable -
--
-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++

-- %CLICMDPREFIX card
-- %CLICMDABBR inventory
-- %CLISHOWGROUP inventory
iShelfCardInventoryTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IShelfCardInventoryEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "table of card equipment information"
    ::= { iShelfCardGroup 2 }

iShelfCardInventoryEntry OBJECT-TYPE
    SYNTAX      IShelfCardInventoryEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "each row identifies a card.  A row is only present if
         the card is known."
    INDEX       { iShelfCardInventoryLocation,
                  iShelfCardInventoryCardType }
    ::= { iShelfCardInventoryTable 1 }

IShelfCardInventoryEntry ::=
    SEQUENCE {
        iShelfCardInventoryLocation
            IShelfLocation,
        iShelfCardInventoryCardType
            IShelfCardType,
        iShelfCardInventoryPartNumber
            IShelfPartNumber,
        iShelfCardInventoryHardwareRevision
            IShelfHardwareRevision,
        iShelfCardInventorySoftwareRevision
            IShelfCardSoftwareRevision,
        iShelfCardInventoryFlashSoftwareRevision
            IShelfCardSoftwareRevision,
        iShelfCardInventoryDate
            IShelfDateTime,
        iShelfCardInventorySerialNumber
            IShelfSerialNumber,
        iShelfCardInventoryFpga
            IShelfFpga,
        iShelfCardInventoryCleiCode
            IShelfCleiCode,
        iShelfCardInventoryProductId
            IShelfProductId,
        iShelfCardInventoryFpga2
            IShelfFpga
    }

-- %CLICOLLABEL Location
-- %CLISHOWGROUP inventory.1
-- %CLISHOWGROUP inventory.2
iShelfCardInventoryLocation OBJECT-TYPE
    SYNTAX      IShelfLocation
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "The node/shelf/slot location of the card."
    ::= { iShelfCardInventoryEntry 1 }

-- %CLICOLLABEL Type
-- %CLISHOWGROUP inventory.1
-- %CLISHOWGROUP inventory.2
iShelfCardInventoryCardType OBJECT-TYPE
    SYNTAX      IShelfCardType
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "This attribute indicates type of card."
    ::= { iShelfCardInventoryEntry 2 }

-- %CLICOLLABEL PartNo
-- %CLISHOWGROUP inventory.1
iShelfCardInventoryPartNumber OBJECT-TYPE
    SYNTAX      IShelfPartNumber
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "This attribute indicates the number used to identify the
         card, options, and its revision number."
    ::= { iShelfCardInventoryEntry 3 }

-- %CLICOLLABEL HWRev
-- %CLISHOWGROUP inventory.1
-- %CLIWIDTH 9
iShelfCardInventoryHardwareRevision OBJECT-TYPE
    SYNTAX      IShelfHardwareRevision
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "This attribute indicates the hardware revision of the card."
    ::= { iShelfCardInventoryEntry 4 }

-- %CLISHOWGROUP inventory.1
iShelfCardInventorySoftwareRevision OBJECT-TYPE
    SYNTAX      IShelfCardSoftwareRevision
    MAX-ACCESS  read-only
    STATUS      current
