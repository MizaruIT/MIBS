    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION ""
    ::= { iShelfCardGroup 3 }

iShelfImageEntry OBJECT-TYPE
    SYNTAX      IShelfImageEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "Shows revision information for running images."
    INDEX       { iShelfImageLocation,
                  iShelfImageCardModule,
                  iShelfImageModule,
                  iShelfImageType }
    ::= { iShelfImageTable 1 }

IShelfImageEntry ::=
    SEQUENCE {
        iShelfImageLocation
