    SYNTAX      INTEGER {
                    main(1),
                    exp1(2)
                }
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "A single slot card will only have a main entry.
         A dual slot card will have a main and exp1 entry."
