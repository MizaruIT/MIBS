-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++

-- %CLICMDPREFIX card
-- %CLICMDABBR ontreminvtable
-- %CLISHOWGROUP ontreminvtable
iShelfOntRemoteInventoryTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IShelfOntRemoteInventoryEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This table provides management access to the ONT remote
         inventory as stored in the ONT non-volatile memory.  An ONT may
         have several remote inventory blocks.  This table is indexed by
         an index value that indicates the block.  There is a remote inventory
         block for each field replacable component.

         Each ONT has a main remote inventory block pertaining to the ONT
         itself.  ONTs supporting expansion cards have remote inventory
         blocks for each expansion card.

         Rows in this table are automatically created and destroyed as ONTs
         are discovered and lost.

         For ONTs supporting expansion cards, entries in this table will
         come and go as cards are inserted and removed, respectively."
    ::= { iShelfOntCard 2 }

iShelfOntRemoteInventoryEntry OBJECT-TYPE
    SYNTAX      IShelfOntRemoteInventoryEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "Each entry corresponds to one remote inventory block."
    INDEX       { iShelfOntRemoteInventoryLocation,
                  iShelfOntRemoteInventoryPonIndex,
                  iShelfOntRemoteInventoryOntId,
                  iShelfOntRemoteInventoryIndex }
    ::= { iShelfOntRemoteInventoryTable 1 }

IShelfOntRemoteInventoryEntry ::=
    SEQUENCE {
        iShelfOntRemoteInventoryLocation
            IShelfLocation,
        iShelfOntRemoteInventoryPonIndex
            IShelfOltPonIndex,
        iShelfOntRemoteInventoryOntId
            IShelfOltOntId,
        iShelfOntRemoteInventoryIndex
            INTEGER,
        iShelfOntRemoteInventoryProductId
            IShelfProductId,
        iShelfOntRemoteInventorySerialNumber
            IShelfOntSerialNumber,
        iShelfOntRemoteInventoryPartNumber
            IShelfPartNumber,
        iShelfOntRemoteInventoryCleiCode
            IShelfCleiCode,
        iShelfOntRemoteInventoryMacAddress1
            MacAddress,
        iShelfOntRemoteInventoryMacAddress2
            MacAddress,
        iShelfOntRemoteInventoryMacAddress3
            MacAddress,
        iShelfOntRemoteInventoryMacAddress4
            MacAddress,
        iShelfOntRemoteInventoryStatus
            INTEGER
    }

