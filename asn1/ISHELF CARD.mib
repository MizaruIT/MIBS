ISHELF-CARD-MIB DEFINITIONS ::= BEGIN

IMPORTS
    OBJECT-TYPE,
    NOTIFICATION-TYPE,
    MODULE-IDENTITY,
    Unsigned32,
    Counter32,
    IpAddress
        FROM SNMPv2-SMI

    RowStatus,
    TruthValue,
    MacAddress
        FROM SNMPv2-TC

    telstrat,
    osiIShelf,
    iShelfCard,
    iShelfTrap,
    iShelfTrapGroup,
    iShelfModules
        FROM ISHELF-ARCS-MIB

    IShelfLabel,
    IShelfLocation,
    IShelfDateTime,
    IShelfCardType,
    IShelfAdminState,
    IShelfOperState,
    IShelfEthernetPortMode,
    IShelfPartNumber,
    IShelfHardwareRevision,
    IShelfSoftwareRevision,
    IShelfCardSoftwareRevision,
    IShelfSerialNumber,
    IShelfFpga,
    IShelfModule,
    IShelfLocation,
    IShelfLocationType,
    IShelfOltOntId,
    IShelfOltPonIndex,
    IShelfOntType,
    IShelfOntRegistrationId,
    IShelfOntSerialNumber,
    IShelfOntFlashLed,
    IShelfOntVoiceMessagePlayback,
    IShelfOntLogicalId,
    IShelfOntDiscLogicalId,
    IShelfOntSubscriberLineCardIndex,
    IShelfOntCustomerLabel,
    IShelfOntSubscriberLineCardType,
    IShelfProductId,
    IShelfCleiCode,
    IShelfOltPonBandwidthRate,
    IShelfOltPonLocalGigE,
    IShelfFilename,
    IShelfCardRedundancy,
    IShelfDisableTableIndex,
	 IShelfOntOperState,
	 IShelfOntSoftwareImageVersion,
	 IShelfOntSoftwareImageIndex,
	 IShelfOntSoftwareImageStatus
        FROM ISHELF-TC
    ;

iShelfCardMIB MODULE-IDENTITY
    LAST-UPDATED "200604280000Z"
    ORGANIZATION "TelStrat"
    CONTACT-INFO "
        TelStrat
        6900 K Avenue
        Plano, TX, USA 75074

        Main Phone: 972-543-3500
        Main Fax: 972-424-2909
        Support Phone: 888-658-8572
        Support Fax: 918-610-9975
        Email:  AccessCustomerSupport@telstrat.com"
    DESCRIPTION "The MIB module for managing Card provisioning."
    REVISION "200604280000Z"
    DESCRIPTION "Beta Release"
    ::= { iShelfModules 3 }

-- %PRIVATEHEADER
-- %CLICMDPREFIX card
-- %CLICMDABBR group
iShelfCardGroup OBJECT IDENTIFIER ::= { iShelfCard 1 }

-- %CLICMDPREFIX card
-- %CLICMDABBR olt
iShelfOltCard OBJECT IDENTIFIER ::= { iShelfCard 2 }

-- %CLICMDPREFIX card
-- %CLICMDABBR ont
iShelfOntCard OBJECT IDENTIFIER ::= { iShelfCard 3 }

-- %CLICMDPREFIX card
-- %CLICMDABBR egw
iShelfEgwCard OBJECT IDENTIFIER ::= { iShelfCard 4 }

-- %CLICMDPREFIX service
-- %CLICMDABBR group
iShelfServicability OBJECT IDENTIFIER ::= { iShelfCard 5 }

-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
--
-- iShelfCardTable -
--
-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
-- %DEEPCPUGET XlateDeepGet 10000 ROW 
-- %ROWSTRUCT USER
-- %CLICMDPREFIX card
-- %CLICMDABBR table
-- %CLISHOWGROUP table
iShelfCardTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IShelfCardEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "the card associated data"
    ::= { iShelfCardGroup 1 }

iShelfCardEntry OBJECT-TYPE
    SYNTAX      IShelfCardEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "each row identifies a card"
    INDEX       { iShelfCardLocation }
    ::= { iShelfCardTable 1 }

IShelfCardEntry ::=
    SEQUENCE {
        iShelfCardLocation
            IShelfLocation,
        iShelfCardExpectedCardType
            IShelfCardType,
        iShelfCardActualCardType
            IShelfCardType,
        iShelfCardAdminState
            IShelfAdminState,
        iShelfCardOperState
            IShelfOperState,
        iShelfCardAction
            INTEGER,
        iShelfCardRedundancyStatus
            IShelfCardRedundancy,
        iShelfCardOltUplink
            INTEGER,
        iShelfCardRowStatus
            RowStatus
    }

-- %CLICOLLABEL location
-- %CLIWIDTH 8
-- %CLISHOWGROUP table
-- %CLICMDABBR location
iShelfCardLocation OBJECT-TYPE
    SYNTAX      IShelfLocation
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "the node/shelf/slot location of the card."
    ::= { iShelfCardEntry 1 }

-- %SETINACTIVE
-- %CLICMDABBR expected
-- %CLICOLLABEL expected
-- %CLISHOWGROUP table
iShelfCardExpectedCardType OBJECT-TYPE
    SYNTAX      IShelfCardType
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "the expected type of the card present in the system.
         NOTE:  this object can only be modified with a non-active rowStatus."
    ::= { iShelfCardEntry 2 }

-- %CLICOLLABEL actual
-- %CLISHOWGROUP table
-- %CLICMDABBR actual
iShelfCardActualCardType OBJECT-TYPE
    SYNTAX      IShelfCardType
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "the actual type of the card present in the system."
    ::= { iShelfCardEntry 3 }

-- %SETINACTIVE
-- %CLIOPTION
-- %CLICMDABBR admin
-- %CLICOLLABEL admin
-- %CLISHOWGROUP table
iShelfCardAdminState OBJECT-TYPE
    SYNTAX      IShelfAdminState
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "the administrative state of the card
         NOTE:  this object can only be modified with a non-active rowStatus."
    DEFVAL      { unlock }
    ::= { iShelfCardEntry 4 }

-- %CLICOLLABEL oper
-- %CLISHOWGROUP table
-- %CLICMDABBR oper
iShelfCardOperState OBJECT-TYPE
    SYNTAX      IShelfOperState
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "the operational state of the card"
    ::= { iShelfCardEntry 5 }

-- %CLIOPTION
-- %CLICOLLABEL action
-- %CLICMDABBR action
iShelfCardAction OBJECT-TYPE
    SYNTAX      INTEGER {
                    noAction(0),
                    resetCard(1)
                }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Setting this will result in the associated
         action being performed on the card.  A Get of this object
         will indicate the last performed action."
    ::= { iShelfCardEntry 6 }

-- %DEEPCPUOBJ
-- %CLICOLLABEL redundancy
-- %CLISHOWGROUP table
-- %CLICMDABBR redundancy
iShelfCardRedundancyStatus OBJECT-TYPE
    SYNTAX      IShelfCardRedundancy
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "The redundancy status of this card."
    ::= { iShelfCardEntry 7 }

-- %SETINACTIVE
-- %CLIOPTION
-- %CLICMDABBR uplink
-- %CLICOLLABEL uplink
-- %CLISHOWGROUP table
iShelfCardOltUplink OBJECT-TYPE
    SYNTAX      INTEGER {
                    default(0),
                    local(1),
                    remote(2),
                    both(3)
                }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "The uplink that is used if this is an OLT card. This object has no effect
         on card types that are not OLT.
         Local will utilize the local GigEthernet port as the uplink.
         Remote will utilize the backplane cell bus to a separate uplink card.
         Both will enable the ability to utilize both the local GigEthernet port
         and the backplane cell bus.
         NOTE:  this object can only be modified with a non-active rowStatus."
    DEFVAL      { default }
    ::= { iShelfCardEntry 8 }

-- %ROWSTATUSTYPE RFC
-- %CLICOLLABEL RowStatus
-- %CLIWIDTH 12
-- %CLISHOWGROUP table
-- %CLICMDABBR rowstatus
iShelfCardRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This object is used for row creation, modification and
         deletion. Modification of objects within the row is only allowed
         when the row is not in the active state. In the case of an active
         row (already created), the row must be made notInService prior to
         modifying any settable objects. The notInService setting has no
         impact on the underlying object but is used to cause atomic updates
         of the row once it is moved to the active state.

         The supported values of RowStatus are:
            active (1)
            notInService (2)
            notReady (3)
            createAndGo (4)
            createAndWait (5)
            destroy (6)"
    ::= { iShelfCardEntry 9 }

-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
