-- %DEEPGET CUSTOM 0 ROW
-- %CLICMDPREFIX card
-- %CLICMDABBR ontprovtable
-- %CLISHOWGROUP ontprovtable
iShelfOntProvisioningTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IShelfOntProvisioningEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "The ONT provisioning table is used for global ONT provisioning.
         Entries in this table are created at the request of the management application.
         The table is indexed by ONT logical ID.

         This table supports pre-provisioning of an ONT.  The management application may
         create an entry in this table before the ONT is present in the system.

         The global administrative state of the ONT is also controlled by this table.
         By setting the administrative state it is possible to enable/disable all
         services provided by the ONT."
    ::= { iShelfOntCard 5 }

iShelfOntProvisioningEntry OBJECT-TYPE
    SYNTAX      IShelfOntProvisioningEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "Each entry contains global provisioning information for a single ONT."
    INDEX       { iShelfOntProvisioningLogicalId }
    ::= { iShelfOntProvisioningTable 1 }

IShelfOntProvisioningEntry ::=
    SEQUENCE {
        iShelfOntProvisioningLogicalId
            IShelfOntLogicalId,
        iShelfOntProvisioningExpectedType
            IShelfOntType,
        iShelfOntProvisioningActualType
            IShelfOntType,
        iShelfOntProvisioningAdminState
            IShelfAdminState,
        iShelfOntProvisioningOperState
            IShelfOntOperState,
        iShelfOntProvisioningSerialNumber
            IShelfOntSerialNumber,
        iShelfOntProvisioningRegistrationId
            IShelfOntRegistrationId,
        iShelfOntProvisioningLocation
            IShelfLocation,
        iShelfOntProvisioningPonIndex
            IShelfOltPonIndex,
        iShelfOntProvisioningOntId
            IShelfOltOntId,
        iShelfOntProvisioningCustomerLabel
            IShelfOntCustomerLabel,
        iShelfOntProvisioningCustomerRecordId
            IShelfLabel,
        iShelfOntProvisioningRowStatus
            RowStatus
    }

