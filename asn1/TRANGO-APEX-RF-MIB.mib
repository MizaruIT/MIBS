	SYNTAX  	INTEGER { qpsk(0), q16q(1), q32q(2), q64q(3), q128q(4), q256q(5) }
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION	"Set/Display the current modulation"
	::= { speed 2 }

