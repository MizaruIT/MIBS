--
--  CRON CAPABILITIES
--
tmnxCronCapsSASM7210V6v0 AGENT-CAPABILITIES
    PRODUCT-RELEASE "Alcatel SAS-M 7210"
    STATUS current
    DESCRIPTION
        "Implementation of CRON scheduling capabilities
         for Alcatel SAS-M 7210 release R6.0"

    SUPPORTS DISMAN-SCHEDULE-MIB
        INCLUDES    {
            schedGroup2,
            schedCalendarGroup,
            schedNotificationsGroup
        }

        VARIATION   schedOwner
            SYNTAX  SnmpAdminString (SIZE (1..32))
            DESCRIPTION
                    "The minimum length of schedOwner is 1 when creating
                     new entries in tables via SNMP SET requests."

        VARIATION   schedInterval
            SYNTAX  Unsigned32 (30..42949672)
            DESCRIPTION
                    "The values supported for schedInterval are between
                    30 and 42949672 seconds.  42949672 seconds creates
                    a maximum interval of 497 days, 2 hours, 27 minutes
                    and 52 seconds."


    SUPPORTS DISMAN-SCRIPT-MIB
        INCLUDES    {
            smLanguageGroup,
            smScriptGroup2,
            smLaunchGroup2,
            smRunGroup2,
            smNotificationsGroup2
        }

        VARIATION   smScriptEntry
            DESCRIPTION
                    "A maximum of 255 smScriptEntry rows are allowed to be
                    created."

        VARIATION   smScriptOwner
            SYNTAX  SnmpAdminString (SIZE (1..32))
            DESCRIPTION
                    "The minimum length of smScriptOwner is 1 when creating
                     new entries in tables via SNMP SET requests."

        VARIATION   smScriptDescr
            SYNTAX  SnmpAdminString (SIZE (0..80))
            DESCRIPTION
                    "The maximum size string supported for smScriptDescr
                    is 80 octets."

        VARIATION   smScriptLanguage
            SYNTAX  Integer32 (1)
            DESCRIPTION
                    "The only script language supported is the TiMOS CLI
                    that has an smLangIndex value equal 1.  smScriptLanguage
                    can only be set to the value 1."

        VARIATION   smScriptSource
            DESCRIPTION
                    "The mib requires that when smScriptAdminStatus is
                    set to 'enabled', the agent at that point should
                    'pull' the script from local cflash or remote URL
                    if one is specified.  This agent does not 'pull'
                    scripts to its local storage.  The URL specified in
                    smScriptSource is passed as an argument to the CLI
                    'exec' command when the smRunState transitions to
                    'executing'.

                    The mib states that an empty URL implies that the
                    script scource is already in local storage and are
                    located by some implementation specific mapping
                    scheme between the local storage file name and the
                    smScriptName index.  This agent does not support
                    this behavior.  An empty smScriptSource URL causes
                    an attempt to set a value to smLaunchStart to return
                    an 'inconsistentValue' error."

        VARIATION   smScriptAdminStatus
            SYNTAX  INTEGER {
                        enabled     (1),
                        disabled    (2)
                        -- editing  (3)
                    }
            DESCRIPTION
                    "The smScriptAdminStatus state 'editing (3)' is not
                    supported because smCodeTable is not supported."

        VARIATION   smScriptStorageType
            DEFVAL  { nonVolatile }
            DESCRIPTION
                    "This agent supports only 'nonVolatile' storage type.
                    This is interpreted to be the desired storage type
                    for the configuration of the script row entry.

                    When smLaunchStorageType has a value of 'nonVolatile'
                    its configuration is written to the configuration
                    file upon a 'save configuration' operation.  If the
                    system reboots between the time this row entry is
                    created and before a 'save configuration' operation is
                    made, this row entry will NOT be restored following
                    the reboot."

        VARIATION   smLaunchEntry
            DESCRIPTION
                    "A maximum of 255 smLaunchEntry rows are allowed to be
                    created."

        VARIATION   smLaunchOwner
            SYNTAX  SnmpAdminString (SIZE (1..32))
            DESCRIPTION
                    "The minimum length of smLaunchOwner is 1 when creating
                     new entries in tables via SNMP SET requests."

        VARIATION   smLaunchMaxRunning
            SYNTAX  Unsigned32 (1)
            DESCRIPTION
                    "Concurrently running scripts are not supported by
                    this agent. Scripts are queued and executed one
                    at a time. An attempt to set smLaunchMaxRunning
                    to any value other than 1 results in an
                    'inconsistentValue' error."

        VARIATION   smLaunchArgument
            DESCRIPTION
                    "This agent uses smLaunchArgument to specify the
                    URL of the file that is created to hold the CLI
                    session output from the 'exec' of the CLI script
                    specified in smScriptSource.  The script specified
                    in smScriptSource is executed as if the 'exec' '-echo'
                    switch was specified in a CLI CONSOLE/Telnet session
                    and the command echo and any display from CLI 'show'
                    commands specified in the script is redirected to the
                    results file specified in smLaunchArgument.

                    The smLaunchArgument URL should consist of the
                    path and a filename prefix.  It refer to either the
                    local cflash drives or a remote file accessed through
                    a remote ftp URL.  When the exec results file is created
                    for a specific run of the script, a timestamp of the
                    form '_yyyymmdd-hhmmss.out' is appended on the end of
                    the smLaunchArgument filename prefix.  For example,
                    setting smLaunchArgument to
                    'ftp://<user>:<passwd>@<a.b.c.d>/cron/results/test-res'
                    causes a script results file to be created named
                    'cron/results/test-res_yyyymmdd-hhmmss.out'.

                    NOTE: The agent does not have any capability to age out,
                    rotate, delete, or otherwise manage these script
                    results files.  It is up to the SR system operator or
                    manager to manage retrieval and cleanup of these files."

        VARIATION   smLaunchExpireTime
            DESCRIPTION
                    "Unlike smLaunchLifeTime, the mib does not specify a
                    value for smLaunchExpireTime that can be used to
                    indicate that an associated smRunEntry with a smRunState
                    equal to 'terminated' should be kept forever and
                    never aged out.  This agent uses the maximum value
                    (2147483647) to specify that a smRunEntry does not
                    age out after its smRunState transitions to
                    'terminated'.  A terminated smRunEntry with a maximum
                    smRunExpireTime value will never be removed from
                    the table."

        VARIATION   smLaunchStart
            DESCRIPTION
                    "In the 4.0 release there is no user access rights
                    checking to verify that the user setting smLaunchStart
                    has read-access to the script.  A user command
                    access profile with RADIUS/TACACS+ authentication will
                    be associated to the script in a later release.

                    The mib requires that in order for a set to smLaunchStart
                    to succede th number of concurrently executing scripts
                    must be less than the value of smLaunchMaxRunning.
                    This agent does not support execution of more than one
                    script at a time.  Instead, a smRunEntry is created
                    with smRunState equal to 'initializing' and sequentially
                    queued for later execution.  The maximum number of
                    smRunEntry rows is limited to 255 regardless of the value
                    of smRunState.

                    The mib specifies that when the smRunEntry is created
                    by setting smLaunchStart, the smRunStartTime should be
                    set to the current time.  Because of the one-at-a-time
                    queuing for execution there may be a long time between
                    entering the 'initializing' state and transitioning
                    to the 'executing' state, this agent does not set
                    smRunStartTime until smRunState transitions to
                    'executing'."

        VARIATION   smLaunchControl
            SYNTAX  INTEGER {
                        abort        (1),
                        -- suspend   (2),
                        -- resume    (3),
                        nop          (4)
                    }
            DESCRIPTION
                    "The 'suspend (2)' and 'resume (3)' controls are not
                    supported.  An attempt to set either value results
                    in an 'inconsistentValue' error."

        VARIATION   smLaunchAdminStatus
            SYNTAX  INTEGER {
                        enabled      (1),
                        disabled     (2)
                        -- autostart (3)
                    }
            DESCRIPTION
                    "The 'autostart (3)' state is not supported.

                    The mib is not clear if smLaunchAdminStatus can be
                    set to 'disabled (2)' if there are associated entries
                    in the smRunTable.  This agent does not allow
                    smLaunchAdminStatus to be set to 'disabled (2)' if there
                    are any associated entries in the smRunTable.  Any
                    associated smRunEntry must be aborted and aged out
                    before smLaunchAdminStatus can be set to 'disabled'."

        VARIATION   smLaunchRunIndexNext
            DESCRIPTION
                    "The mib requires that subsequent reads of
                    smLaunchRunIndexNext must return different values.
                    This agent does not increment the value with every
                    read.  A read always returns the next available
                    index value."

        VARIATION   smLaunchStorageType
            SYNTAX  INTEGER {
                        -- other        (1),
                        -- volatile     (2),
                        nonVolatile     (3)  -- e.g., in NVRAM
                        -- permanent    (4),
                        -- readOnly     (5)
                    }
            DEFVAL  { nonVolatile }
            DESCRIPTION
                    "This agent supports only 'nonVolatile (3)' storage type.
                    This is interpreted to be the desired storage type
                    for the configuration of the launch entry.

                    When smLaunchStorageType has a value of 'nonVolatile'
                    its configuration is written to the configuration
                    file upon a 'save configuration' operation.  If the
                    system reboots between the time this row entry is
                    created and before a 'save configuration' operation is
                    made, this row entry will NOT be restored following
                    the reboot."

        VARIATION   smLaunchRowExpireTime
            ACCESS  read-only
            DESCRIPTION
                    "smLaunchRowExpireTime functionality is not supported.
                    Its value cannot be modified.  The default value
                    2147483647 is always returned to indicate that the
                    smLaunchTable row entry will never be aged out."

        VARIATION   smRunEntry
            DESCRIPTION
                    "A maximum of 255 smRunEntry rows are allowed to be created
                    regardless of the value of smRunState."

        VARIATION   smRunArgument
            SYNTAX  OCTET STRING (SIZE (0..255))
            DESCRIPTION
                    "The maximum size string returned by a read of
                    smRunArgument is 255 octets."

        VARIATION   smRunLifeTime
            DESCRIPTION
                    "It is not clear from the mib if smRunLifeTime starts
                    to tick backwards when smRunState equals 'initializing'
                    or when smRunState equals 'executing'.  This agent
                    implementation starts to decrement smRunLifeTime
                    when smRunState transitions to 'executing'."

        VARIATION   smRunExpireTime
            DESCRIPTION
                    "Unlike smRunLifeTime, the mib does not specify a
                    value for smRunExpireTime that can be used to
                    indicate this smRunEntry should be kept forever and
                    never aged out.  This agent uses the maximum value
                    (2147483647) to specify that this smRunEntry does not
                    age out after its smRunState transitions to
                    'terminated'.

                    If smRunExpireTime is set to its maximum value
                    (2147483647), either by a set operation or by its
                    initialization from the smLaunchExpireTime object,
                    then it does not tick backwards. A terminated script
                    with a maximum smRunExpireTime value thus is never
                    be aged out and removed from the table."

        VARIATION   smRunResult
            SYNTAX  OCTET STRING (SIZE (0..255))
            DESCRIPTION
                    "The maximum size string returned by a read of
                    smRunResult is 255 octets."

        VARIATION   smRunControl
            SYNTAX  INTEGER {
                        abort       (1),
                        -- suspend  (2),
                        -- resume   (3),
                        nop         (4)
                    }
            DESCRIPTION
                    "The 'suspend (2)' and 'resume (3)' controls are not
                     supported.  An attempt to set either value results in
                     an 'inconsistentValue' error."

        VARIATION   smRunState
            SYNTAX INTEGER {
                        initializing    (1),
                        executing       (2),
                        -- suspending   (3),
                        -- suspended    (4),
                        -- resuming     (5),
                        aborting        (6),
                        terminated      (7)
                    }
            DESCRIPTION
                    "The 'suspending (3)', 'suspended (4)', and 'resuming (5)'
                    states are not supported."

        VARIATION   smRunStartTime
            DESCRIPTION
                    "It is not clear from the mib if smRunStartTime is
                    set when smRunState transitions to 'initializing' or
                    not until smRunState transitions to 'executing'.
                    This agent implementation sets smRunStartTime when
                    smRunState transitions to 'executing'."

    ::= { tmnxSASM7210V6v0 35 }  -- CRON DISMAN-SCHEDULE-MIB & DISMAN-SCRIPT-MIB

