--
--  REMOTE MONITORING, RMON, CAPABILITIES
--
tmnxRmonCapsSASM7210V6v0 AGENT-CAPABILITIES
    PRODUCT-RELEASE "Alcatel SAS-M 7210"
    STATUS current
    DESCRIPTION
        "Implementation of Remote Monitoring, RMON,
         capabilities for Alcatel SAS-M 7210 release R6.0"

    SUPPORTS RMON-MIB
        INCLUDES    {
            rmonEtherStatsGroup,
            rmonHistoryControlGroup,
            rmonEthernetHistoryGroup,
            rmonAlarmGroup,
            rmonEventGroup
            -- rmonHostGroup :
            --   EXCLUDE hostControlTable
            --   EXCLUDE hosts
            -- rmonHostTopNGroup
            --   EXCLUDE hostTopN
            -- rmonMatrixGroup
            --   EXCLUDE matrix
            -- rmonFilterGroup
            --   EXCLUDE filter
            -- rmonPacketCaptureGroup
            --   EXCLUDE capture
            -- rmonNotificationGroup
            --   EXCLUDE rmonConformance
        }

        VARIATION   etherStatsIndex
            SYNTAX  InterfaceIndex
            DESCRIPTION
                    "The agent creates a row entry in the etherStatsTable for
                    each ethernet port in the system that is indexed by the
                    port-id or ifIndex value which has a larger value range
                    than that specified for etherStatsIndex in the RMON-MIB."

        VARIATION   etherStatsDataSource
            ACCESS  read-only
            DESCRIPTION
                    "read-only implementation. The agent creates a row entry
                     in the etherStatsTable for each ethernet port in the
                     system, indexed by the port-id."

        VARIATION   etherStatsOwner
            ACCESS  read-only
            DESCRIPTION
                    "read-only implementation."

        VARIATION   etherStatsStatus
            ACCESS  read-only
            DESCRIPTION
                    "etherStatsTable is read-only on SAS-M 7210. Rows cannot
                    be created by SNMP SET requests. The agent creates a row
                    entry in the etherStatsTable for each ethernet port in
                    the system, indexed by the port-id."

        VARIATION   etherStatsOversizePkts
            DESCRIPTION
                    "etherStatsOversizePkts indicates the total number of
                    packets received that were longer than the maximum frame
                    size for the particular media, usually 12287 octets
                    (excluding framing bits, but including FCS octets), and
                    were otherwise well formed.

                    TIMETRA-PORT-MIB::tmnxPortEtherPkts1519toMax,
                    TIMETRA-PORT-MIB::tmnxPortEtherHCOverPkts1519toMax and
                    TIMETRA-PORT-MIB::tmnxPortEtherHCPkts1519toMax are used
                    to indicate the total number of packets received that were
                    longer than 1518 octets but less than the maximum frame
                    size for the particular media, usually 12287 octets
                    (excluding framing bits, but including FCS octets)."

    SUPPORTS HC-RMON-MIB
        INCLUDES    {
            mediaIndependentGroup,
            etherStatsHighCapacityGroup,
            etherHistoryHighCapacityGroup,
            hcRMONInformationGroup
        }

        VARIATION   mediaIndependentIndex
            SYNTAX  InterfaceIndex
            DESCRIPTION
                    "The agent creates a row entry in the
                    mediaIndependentTable for each physical port in
                    the system, indexed by the port-id which is also
                    the ifIndex which has a larger value range than
                    that specified for mediaIndependentIndex in the
                    HC-RMON-MIB."

        VARIATION   mediaIndependentDataSource
            ACCESS  read-only
            DESCRIPTION
                    "mediaIndependentTable is read-only on SAS-M 7210.
                    The agent creates a row entry in the mediaIndependentTable
                    for each physical port in the system, indexed by the
                    port-id."

        VARIATION   mediaIndependentOwner
            ACCESS  read-only
            DESCRIPTION
                    "read-only implementation."

        VARIATION   mediaIndependentStatus
            ACCESS  read-only
            DESCRIPTION
                    "mediaIndependentTable is read-only on SAS-M 7210.
                    Rows cannot be created in the table by SNMP SET requests.
                    The agent creates a row entry in the mediaIndependentTable
                    for each physical port in the system, indexed by the
                    port-id."

    ::= { tmnxSASM7210V6v0 6 }

