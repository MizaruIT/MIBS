            -- EXCLUDE ipv6RouterAdvertSpinLock (not supported)
            -- EXCLUDE icmpStatsTable (not supported)
            -- EXCLUDE icmpMsgStatsTable (not supported)
        }

        VARIATION   ipNetToMediaType
            SYNTAX  INTEGER {
                        other   (1),      -- none of the following
                        invalid (2),      -- an invalidated mapping
                        dynamic (3),
                        static  (4),
                        managed (5)       -- added for 7x50
                    }
            DESCRIPTION
                    "The value 'managed (5)' is used to indicate that the
                    ARP entry has been added by:

                    - a static host; or
                    - by a DHCP lease state if
                      TIMETRA-VRTR-MIB::vRtrIfArpPopulate is set to enabled.

                    An attempt to explicitly set the value of ipNetToMediaType
                    to 'managed' results in an 'inconsistentValue' error."

        VARIATION   ipForwarding
            ACCESS  read-only
            DESCRIPTION
                    "ipForwarding is read-only on SAS-M 7210. It always
                     returns the value 'forwarding (1)'."

        VARIATION   ipDefaultTTL
            ACCESS  read-only
            DESCRIPTION
                    "ipDefaultTTL is read-only on SAS-M 7210. It always
                     returns the value '64'."

        VARIATION   ipv6RouterAdvertSpinLock
            ACCESS  not-implemented
            DESCRIPTION
                    "Not supported."

        VARIATION   ipNetToPhysicalType
            SYNTAX  INTEGER {
                        other   (1),      -- none of the following
                        invalid (2),      -- an invalidated mapping
                        dynamic (3),
                        static  (4),
                        local   (5),      -- local interface
                        managed (100)     -- added for 7x50
                    }
            DESCRIPTION
                    "The value 'managed (100)' is used to indicate that the
                    ARP entry has been added by:

                    - a static host; or
                    - by a DHCP lease state if
                      TIMETRA-VRTR-MIB::vRtrIfArpPopulate is set to enabled.

                    An attempt to explicitly set the value of
                    ipNetToPhysicalType to 'managed' results in an
                    'inconsistentValue' error."


