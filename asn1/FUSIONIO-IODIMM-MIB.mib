    fusionIoDimmInfoMilliVoltsMin            INTEGER,
    fusionIoDimmInfoMilliWatts               INTEGER,
    fusionIoDimmInfoMilliWattsPeak           INTEGER,
    fusionIoDimmInfoMilliAmps                INTEGER,
    fusionIoDimmInfoMilliAmpsPeak            INTEGER,
    fusionIoDimmInfoAdapterType              INTEGER,
    fusionIoDimmInfoAdapterPort              INTEGER,
    fusionIoDimmInfoAdapterSerialNumber      DisplayString,
    fusionIoDimmInfoAdapterExtPowerPresent   Boolean,
    fusionIoDimmInfoPowerlossProtectDisabled Boolean,
    fusionIoDimmInfoInternalTempHigh         Boolean,
    fusionIoDimmInfoAmbientTemp              INTEGER,
    fusionIoDimmInfoPCIBandwidthCompatibility  INTEGER,
    fusionIoDimmInfoPCIPowerCompatibility      INTEGER,
    fusionIoDimmInfoActualGoverningLevel       INTEGER,
