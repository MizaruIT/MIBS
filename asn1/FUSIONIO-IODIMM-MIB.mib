-- SimpleTime :== TEXTUAL-CONVENTION
--      SYNTAX  DisplayString
--      STATUS  mandatory
--      DESCRIPTION
--
--      "The date and time in text format:
--           YYYY/MM/DD-HH:MM:SS
--
--      May be simplified by expressing only the date."

SimpleTime ::= DisplayString

Boolean ::= INTEGER {
                true(1),
                false(2) }

