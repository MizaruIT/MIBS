tlsTrunkGroupStatsLinkDownRxMsg  OBJECT-TYPE
        SYNTAX        Counter32
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION   "The number of Link Down messages received from the 
        	       remote trunk."
        ::= { tlsTrunkGroupStatsEntry 5}

tlsTrunkGroupStatsLinkUpTxMsg    OBJECT-TYPE
        SYNTAX        Counter32
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION   "The number of Link Up messages sent by the local 
        	       trunk."
        ::= { tlsTrunkGroupStatsEntry 6}

tlsTrunkGroupStatsLinkUpRxMsg    OBJECT-TYPE
        SYNTAX        Counter32
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION   "The number of Link Up messages received from the
                       remote trunk."
        ::= { tlsTrunkGroupStatsEntry 7}

