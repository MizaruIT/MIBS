        fscDdmMpMpStatus        INTEGER,
        fscDdmMpMpPreferred     INTEGER,
        fscDdmMpDwName          DisplayString,
        fscDdmMpDwGroupNumber   INTEGER,
        fscDdmMpScPort          INTEGER,
        fscDdmMpScPath          INTEGER,
        fscDdmMpScTarget        INTEGER,
        fscDdmMpScLun           INTEGER,
        fscDdmMpScDiskNumber    INTEGER,
        fscDdmMpScSize          INTEGER,
        fscDdmMpScSignature     INTEGER,
        fscDdmMpScType          INTEGER,
        fscDdmMpScInquiry       DisplayString,
        fscDdmMpScResource      DisplayString,
        fscDdmMpScNode          DisplayString,
        fscDdmMpScPciBus        INTEGER,
        fscDdmMpScPciDevice     INTEGER,
        fscDdmMpScPciFunction   INTEGER,
        fscDdmMpScDriverName    DisplayString,
        fscDdmMpScDriverDescription DisplayString
    }

