-- **********************************************************************************************

-- GROUP: fscDdmStatus
--
-- This group defines DDM global information

-- **********************************************************************************************


-- Oid: .1.3.6.1.4.1.231.2.10.2.2.9.2.1.0
fscDdmLastDate      OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION "Date of last recent reconfiguration in time_t (seconds since 1/1/1970 00:00:00)."
            ::= { fscDdmStatus 1 }

-- Oid: .1.3.6.1.4.1.231.2.10.2.2.9.2.2.0
fscDdmCurrentStatus OBJECT-TYPE
    SYNTAX  INTEGER {
        ok (1),
        degraded (2),
        error (3),
        failed (4)
    }
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION "Overall status of the driver, agent and the underlying objects.
                Possible values are:
                OK everything is fine
                DEGRADED at least one MultiPath or DuplexWrite group is in a degraded state (e.g. piece failed)
                ERROR software exception occured (software bug)
                FAILED communication with driver failed (e.g. driver not installed)"
            ::= { fscDdmStatus 2 }

-- Oid: .1.3.6.1.4.1.231.2.10.2.2.9.2.3.0
fscDdmInstallationType  OBJECT-TYPE
    SYNTAX  INTEGER {
        ddewin (1),
        ddmwin (2),
        mpwin (3),
        ddelx (4),
        ddmlx (5),
        mplx (6),
        unknown (99)
    }
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION "Type of installed DuplexDataManager edition.
                Possible values are:
                DDMWIN PRIMERGY DuplexDataManager(R) (Windows)
                DDEWIN PRIMERGY easyMirror(TM) (Windows)
                MPWIN PRIMERGY MultiPath (Windows)
                DDMLX PRIMERGY DuplexDataManager(R) (Linux)
                DDELX PRIMERGY easyMirror(TM) (Linux)
                MPLX PRIMERGY MultiPath (Linux)
                UNKNOWN none of the above"
            ::= { fscDdmStatus 3 }


-- **********************************************************************************************

-- GROUP: fscDdmDwContainer
--
-- The DuplexDataManager SNMP agent's container for DuplexWrite info

-- **********************************************************************************************


fscDdmDwPieceTable  OBJECT-TYPE
    SYNTAX  SEQUENCE OF FscDdmDwPiece
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION "Table of all pieces DuplexWrite has detected."
            ::= { fscDdmDwContainer 1}

fscDdmDwPiece       OBJECT-TYPE
    SYNTAX  FscDdmDwPiece
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION "Entry in table of DuplexWrite pieces."
    INDEX   { fscDdmDwIndex }
            ::= { fscDdmDwPieceTable 1}

FscDdmDwPiece       ::= SEQUENCE {
        fscDdmDwIndex           INTEGER,
        fscDdmDwDwPort          INTEGER,
        fscDdmDwDwPath          INTEGER,
        fscDdmDwDwTarget        INTEGER,
        fscDdmDwDwLun           INTEGER,
        fscDdmDwDwName          DisplayString,
        fscDdmDwDwGroupNumber   INTEGER,
        fscDdmDwDwPieceNumber   INTEGER,
        fscDdmDwDwSize          INTEGER,
        fscDdmDwDwRoot          INTEGER,
        fscDdmDwDwRecoverMode   INTEGER,
        fscDdmDwDwStatus        INTEGER,
        fscDdmDwDwReason        INTEGER,
        fscDdmDwDwActive        INTEGER,
        fscDdmDwDwRecoverProgress   INTEGER,
        fscDdmDwDwHidden        INTEGER,
        fscDdmDwDwRecent        INTEGER,
        fscDdmDwDwForced        INTEGER,
        fscDdmDwDwPieceCount    INTEGER,
        fscDdmDwDwPieceFound    INTEGER,
        fscDdmDwDwBalance       INTEGER,
        fscDdmDwMpGroupNumber   INTEGER,
        fscDdmDwScPort          INTEGER,
        fscDdmDwScPath          INTEGER,
        fscDdmDwScTarget        INTEGER,
        fscDdmDwScLun           INTEGER,
        fscDdmDwScDiskNumber    INTEGER,
        fscDdmDwScSize          INTEGER,
        fscDdmDwScSignature     INTEGER,
        fscDdmDwScType          INTEGER,
        fscDdmDwScInquiry       DisplayString,
        fscDdmDwScResource      DisplayString,
        fscDdmDwScNode          DisplayString,
        fscDdmDwScPciBus        INTEGER,
        fscDdmDwScPciDevice     INTEGER,
        fscDdmDwScPciFunction   INTEGER,
        fscDdmDwScDriverName    DisplayString,
        fscDdmDwScDriverDescription DisplayString
    }

