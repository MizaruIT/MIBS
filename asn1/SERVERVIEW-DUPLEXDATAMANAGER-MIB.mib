    DESCRIPTION "Internal group number of the MultiPath group."
            ::= { fscDdmMpPiece 6 }

-- Oid: .1.3.6.1.4.1.231.2.10.2.2.9.4.1.1.7.0
fscDdmMpMpAutoRecovery  OBJECT-TYPE
    SYNTAX  INTEGER {
        on (1),
        off (2),
        unknown (99)
    }
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION "AutoRecovery status of the MultiPath group.
                When AutoRecovery is turned on a repaired connection returns into a MultiPath group as Active or Standby otherwise it remains in status Error.
                Possible values are:
                ON AutoRecovery is turned on
                OFF AutoRecovery is turned off
                UNKNOWN none of the above"
            ::= { fscDdmMpPiece 7 }

-- Oid: .1.3.6.1.4.1.231.2.10.2.2.9.4.1.1.8.0
fscDdmMpMpAutoFailback  OBJECT-TYPE
    SYNTAX  INTEGER {
        on (1),
        off (2),
        unknown (99)
    }
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION "AutoFailback status of the MultiPath group.
                If AutoFailback is turned on a repaired connection returns into a MultiPath group in the state it was before the error occured.
                Possible values are:
                ON AutoFailback is turned on
                OFF AutoFailback is turned off
                UNKNOWN none of the above"
            ::= { fscDdmMpPiece 8 }

-- Oid: .1.3.6.1.4.1.231.2.10.2.2.9.4.1.1.9.0
fscDdmMpMpCabinetHandling   OBJECT-TYPE
    SYNTAX  INTEGER {
        default (1),
        clariion (2),
        unknown (99)
    }
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION "Connected storage type. If this cannot be detected the value is UNKNOWN.
                Possible values are:
                DEFAULT S40, JBOD
                CLARIION FibreCAT
                UNKNOWN none of the above"
            ::= { fscDdmMpPiece 9 }

-- Oid: .1.3.6.1.4.1.231.2.10.2.2.9.4.1.1.10.0
fscDdmMpMpStatus    OBJECT-TYPE
    SYNTAX  INTEGER {
        active (1),
        standby (2),
        error (3),
        disabled (4),
        unknown (99)
    }
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION "Status of the MultiPath path.
                Possible values are:
                ACTIVE the piece can accept I/Os. When LoadBalancing is off only one path of a MultiPath group can be active
                STANDBY the path waits until an active path fails and takes over all I/Os then
                ERROR the path failed due to an error
                DISABLED the path is turned off - no I/O is possible
                UNKNOWN none of the above"
            ::= { fscDdmMpPiece 10 }

-- Oid: .1.3.6.1.4.1.231.2.10.2.2.9.4.1.1.11.0
fscDdmMpMpPreferred OBJECT-TYPE
    SYNTAX  INTEGER {
        on (1),
        off (2),
        unknown (99)
    }
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION "Flag which path of a MultiPath group will be active after an error situation is cleared.
                This flag is only valid in case of AutoFailback.
                Possible values are:
                ON use the path as preferred MultiPath path
                OFF not marked as the preferred MultiPath path
                UNKNOWN none of the above"
            ::= { fscDdmMpPiece 11 }

