--
-- QoS subscriber transmit queue objects
--

eriRouterQosSubscriberQueueStatsTable   OBJECT-TYPE
    SYNTAX      SEQUENCE OF EriRouterQosSubscriberQueueStatsEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This table specifies per transmit queue statistical information.
         It contains the transmit queue packet/octet counts for each active
         subscriber session for which QoS queuing policy is enabled."
    ::= { eriRouterQosMibObjects 3 }

eriRouterQosSubscriberQueueStatsEntry OBJECT-TYPE
    SYNTAX      EriRouterQosSubscriberQueueStatsEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "A conceptual row in the eriRouterQosSubscriberQueueStatsTable. Each row
         contains the transmitted and dropped packets/octets count for
         each transmit queue used on a subscriber circuit."
    INDEX { eriRouterSubsActiveName, eriRouterSubsActiveSessionId, eriRouterQosSubsQueueId }
    ::= { eriRouterQosSubscriberQueueStatsTable 1 }
    
EriRouterQosSubscriberQueueStatsEntry ::= SEQUENCE {
    eriRouterQosSubsQueueId                     Unsigned32,
    eriRouterQosSubsQueuePolicyName             SnmpAdminString,
    eriRouterQosSubsQueueOutOctets              Counter32,
    eriRouterQosSubsQueueOutPkts                Counter32,
    eriRouterQosSubsQueueWredDroppedOctets      Counter32,
    eriRouterQosSubsQueueWredDroppedPkts        Counter32,
    eriRouterQosSubsQueueTailDroppedOctets      Counter32,
    eriRouterQosSubsQueueTailDroppedPkts        Counter32,
    eriRouterQosSubsQueueHCOutOctets            Counter64,
    eriRouterQosSubsQueueHCOutPkts              Counter64,
    eriRouterQosSubsQueueHCWredDroppedOctets    Counter64,
    eriRouterQosSubsQueueHCWredDroppedPkts      Counter64,
    eriRouterQosSubsQueueHCTailDroppedOctets    Counter64,
    eriRouterQosSubsQueueHCTailDroppedPkts      Counter64,
    eriRouterQosSubsQueueIPv4OutOctets              Counter32,
    eriRouterQosSubsQueueIPv4OutPkts                Counter32,
    eriRouterQosSubsQueueIPv4WredDroppedOctets      Counter32,
    eriRouterQosSubsQueueIPv4WredDroppedPkts        Counter32,
    eriRouterQosSubsQueueIPv4TailDroppedOctets      Counter32,
    eriRouterQosSubsQueueIPv4TailDroppedPkts        Counter32,
    eriRouterQosSubsQueueHCIPv4OutOctets            Counter64,
    eriRouterQosSubsQueueHCIPv4OutPkts              Counter64,
    eriRouterQosSubsQueueHCIPv4WredDroppedOctets    Counter64,
    eriRouterQosSubsQueueHCIPv4WredDroppedPkts      Counter64,
    eriRouterQosSubsQueueHCIPv4TailDroppedOctets    Counter64,
    eriRouterQosSubsQueueHCIPv4TailDroppedPkts      Counter64,
    eriRouterQosSubsQueueIPv6OutOctets              Counter32,
    eriRouterQosSubsQueueIPv6OutPkts                Counter32,
    eriRouterQosSubsQueueIPv6WredDroppedOctets      Counter32,
    eriRouterQosSubsQueueIPv6WredDroppedPkts        Counter32,
    eriRouterQosSubsQueueIPv6TailDroppedOctets      Counter32,
    eriRouterQosSubsQueueIPv6TailDroppedPkts        Counter32,
    eriRouterQosSubsQueueHCIPv6OutOctets            Counter64,
    eriRouterQosSubsQueueHCIPv6OutPkts              Counter64,
    eriRouterQosSubsQueueHCIPv6WredDroppedOctets    Counter64,
    eriRouterQosSubsQueueHCIPv6WredDroppedPkts      Counter64,
    eriRouterQosSubsQueueHCIPv6TailDroppedOctets    Counter64,
    eriRouterQosSubsQueueHCIPv6TailDroppedPkts      Counter64
}

