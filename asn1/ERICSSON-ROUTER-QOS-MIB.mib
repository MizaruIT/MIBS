ERICSSON-ROUTER-QOS-MIB DEFINITIONS ::= BEGIN

IMPORTS
    ifIndex
            FROM IF-MIB

    Counter32,
    Counter64,
    MODULE-IDENTITY,
    OBJECT-TYPE,
    Unsigned32
            FROM SNMPv2-SMI

    MODULE-COMPLIANCE,
    OBJECT-GROUP
            FROM SNMPv2-CONF

    SnmpAdminString
            FROM SNMP-FRAMEWORK-MIB

    eriRouterSubsActiveName,
    eriRouterSubsActiveSessionId
            FROM ERICSSON-ROUTER-SUBSCRIBER-ACTIVE-MIB

    eriRouterMgmt
            FROM ERICSSON-ROUTER-SMI

    TEXTUAL-CONVENTION
            FROM SNMPv2-TC;


eriRouterQosMib MODULE-IDENTITY
    LAST-UPDATED "201501141800Z" -- Jan 14, 2015
    ORGANIZATION "Ericsson AB."
    CONTACT-INFO
        "         Ericsson AB.
          Postal: 100 Headquarters Dr.
                  San Jose, CA  95134
                  USA

           Phone: +1 408 750 5000
             Fax: +1 408 750 5599
        "
    DESCRIPTION
        "This MIB defines statistical counters that may be used to monitor
         subscriber circuits and those IF-MIB layers, which support
         ifPacketGroup, ifHCPacketGroup or ifVHCPacketGroup, that have
         Quality Of Service enabled.
        "

    REVISION      "201501141800Z" -- Jan 14, 2015
    DESCRIPTION
        "Updated RBN/rbn/Rbn as ERICSSON-ROUTER/eriRouter/EriRouter
         to migrate RBN MIB to ERICSSON MIB."

    REVISION      "201101191800Z" -- Jan 19, 2011
    DESCRIPTION
        "Update CONTACT-INFO & ORGANIZATION. "

    REVISION      "200707300000Z" -- July 30, 2007
    DESCRIPTION
        "Added two new tables, eriRouterQosHierarchicalPolicyStatsTable and
         eriRouterQosHierarchicalPClassStatsTable, containing drop counts
         due to hierarchical policies or policy classes applied to a
         child circuit.
        "

    REVISION      "200609120000Z" -- September 12, 2006
    DESCRIPTION
        "Added two new tables, eriRouterQosIntfRLClassStatsTable and
         eriRouterQosSubscriberRLClassStatsTable, to monitor interfaces and
         subscriber circuits that have policing/metering policy class
         counters enabled.
        "

    REVISION      "200509260000Z" -- September 26, 2005
    DESCRIPTION
        "Added a new table eriRouterQosSubscriberQueueStatsTable to monitor the
         qos statistics per transmit queue on a subscriber circuit.
        "

    REVISION      "200307180000Z" -- July 18, 2003
    DESCRIPTION
        "Added a new table eriRouterQosInterfaceQueueStatsTable to monitor the
         qos statistics per transmit queue on an interface.
        "

    REVISION      "200203220000Z" -- March 22, 2002
    DESCRIPTION
        "Initial version."
    ::= { eriRouterMgmt 22 }


eriRouterQosMibObjects
        OBJECT IDENTIFIER ::= { eriRouterQosMib 1 }

eriRouterQosMibConformance
        OBJECT IDENTIFIER ::= { eriRouterQosMib 2 }

--
-- Textual Conventions
--

EriRouterQosPolicyType ::= TEXTUAL-CONVENTION
    STATUS       current
    DESCRIPTION
        "QoS policy type"
    SYNTAX      INTEGER {
                    policing(1), -- inbound policing
                    metering(2), -- outbound
                    edrr(3),     -- enhanced deficit round robin
                    mdrr(4),     -- modified deficit round robin
                    pq(5),       -- priority queuing
                    atmwfq(6),   -- ATM weighted fair queuing
                    pwfq(7)      -- priority weighted fair queuing
                }

EriRouterQosClassId ::= TEXTUAL-CONVENTION
    DISPLAY-HINT "d"
    STATUS       current
    DESCRIPTION
        "A unique QoS class identifier."
    SYNTAX  Unsigned32 (1..8)

--
-- QoS interface objects
--

eriRouterQosInterfaceTable   OBJECT-TYPE
    SYNTAX      SEQUENCE OF EriRouterQosInterfaceEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This table contains entries for every packet oriented 
         IF-MIB layer for which QoS is enabled."
    ::= { eriRouterQosMibObjects 1 }

eriRouterQosInterfaceEntry OBJECT-TYPE
    SYNTAX      EriRouterQosInterfaceEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "A conceptual row in the eriRouterQosInterfaceTable."
    INDEX { ifIndex }
    ::= { eriRouterQosInterfaceTable 1 }
    
EriRouterQosInterfaceEntry ::= SEQUENCE {
    eriRouterQosIfInOctets            Counter32,
    eriRouterQosIfHCInOctets          Counter64,
    eriRouterQosIfOutOctets           Counter32,
    eriRouterQosIfHCOutOctets         Counter64,
    eriRouterQosIfInDroppedOctets     Counter32,
    eriRouterQosIfHCInDroppedOctets   Counter64,
    eriRouterQosIfOutDroppedOctets    Counter32,
    eriRouterQosIfHCOutDroppedOctets  Counter64
}

eriRouterQosIfInOctets OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
           "The total number of post-limited octets received on the interface,
            including framing characters.

            Discontinuities in the value of this counter can occur at
            re-initialization of the management system, and at other
            times as indicated by the value of
            ifCounterDiscontinuityTime."
    ::= { eriRouterQosInterfaceEntry 1}

eriRouterQosIfHCInOctets OBJECT-TYPE
    SYNTAX      Counter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
           "The total number of post-limited octets received on the interface,
            including framing characters.  This object is 64-bit version of
            eriRouterQosIfInOctets.

            Discontinuities in the value of this counter can occur at
            re-initialization of the management system, and at other
            times as indicated by the value of
            ifCounterDiscontinuityTime."
    ::= { eriRouterQosInterfaceEntry 2}

eriRouterQosIfOutOctets OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
           "The total number of pre-limited octets transmitted out of the
            interface, including framing characters.

            Discontinuities in the value of this counter can occur at
            re-initialization of the management system, and at other
            times as indicated by the value of
            ifCounterDiscontinuityTime."
    ::= { eriRouterQosInterfaceEntry 3}

eriRouterQosIfHCOutOctets OBJECT-TYPE
    SYNTAX      Counter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
           "The total number of pre-limited octets transmitted out of the
            interface, including framing characters.  This object is a
            64-bit version of eriRouterQosIfOutOctets.

            Discontinuities in the value of this counter can occur at
            re-initialization of the management system, and at other
            times as indicated by the value of
            ifCounterDiscontinuityTime."
    ::= { eriRouterQosInterfaceEntry 4 }

eriRouterQosIfInDroppedOctets OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
           "The total number of received octets that were dropped on the 
            interface, including but not limited to octets dropped due to 
            limiting.

            Discontinuities in the value of this counter can occur at
            re-initialization of the management system, and at other
            times as indicated by the value of
            ifCounterDiscontinuityTime."
    ::= { eriRouterQosInterfaceEntry 5 }

eriRouterQosIfHCInDroppedOctets OBJECT-TYPE
    SYNTAX      Counter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
           "The total number of received octets that were dropped on the 
            interface, including but not limited to octets dropped due to 
            limiting. This object is a 64-bit version of 
            eriRouterQosIfInDroppedOctets.

            Discontinuities in the value of this counter can occur at
            re-initialization of the management system, and at other
            times as indicated by the value of
            ifCounterDiscontinuityTime."
    ::= { eriRouterQosInterfaceEntry 6 }


eriRouterQosIfOutDroppedOctets OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
           "The total number of outbound octets that were dropped on the 
            interface, including but not limited to octets dropped due to 
            limiting.

            Discontinuities in the value of this counter can occur at
            re-initialization of the management system, and at other
            times as indicated by the value of
            ifCounterDiscontinuityTime."
    ::= { eriRouterQosInterfaceEntry 7 }

eriRouterQosIfHCOutDroppedOctets OBJECT-TYPE
    SYNTAX      Counter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
           "The total number of outbound octets that were dropped on the 
            interface, including but not limited to octets dropped due to 
            limiting. This object is a 64-bit version of 
            eriRouterQosIfOutDroppedOctets.

            Discontinuities in the value of this counter can occur at
            re-initialization of the management system, and at other
            times as indicated by the value of
            ifCounterDiscontinuityTime."
    ::= { eriRouterQosInterfaceEntry 8 }

--
-- QoS interface transmit queue objects
--

eriRouterQosInterfaceQueueStatsTable   OBJECT-TYPE
    SYNTAX      SEQUENCE OF EriRouterQosInterfaceQueueStatsEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This table specifies per transmit queue statistical information.
         It contains the transmit queue packet/octet counts for each
         packet oriented IF-MIB layer for which QoS is enabled."
    ::= { eriRouterQosMibObjects 2 }

eriRouterQosInterfaceQueueStatsEntry OBJECT-TYPE
    SYNTAX      EriRouterQosInterfaceQueueStatsEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "A conceptual row in the eriRouterQosInterfaceQueueStatsTable. Each row
         contains the transmitted and dropped packets/octets count for
         each transmit queue on an interface."
    INDEX { ifIndex, eriRouterQosIfQueueId }
    ::= { eriRouterQosInterfaceQueueStatsTable 1 }
    
EriRouterQosInterfaceQueueStatsEntry ::= SEQUENCE {
    eriRouterQosIfQueueId                     Unsigned32,
    eriRouterQosIfQueueOutOctets              Counter32,
    eriRouterQosIfQueueOutPkts                Counter32,
    eriRouterQosIfQueueWredDroppedOctets      Counter32,
    eriRouterQosIfQueueWredDroppedPkts        Counter32,
    eriRouterQosIfQueueTailDroppedOctets      Counter32,
    eriRouterQosIfQueueTailDroppedPkts        Counter32,
    eriRouterQosIfQueueHCOutOctets            Counter64,
    eriRouterQosIfQueueHCOutPkts              Counter64,
    eriRouterQosIfQueueHCWredDroppedOctets    Counter64,
    eriRouterQosIfQueueHCWredDroppedPkts      Counter64,
    eriRouterQosIfQueueHCTailDroppedOctets    Counter64,
    eriRouterQosIfQueueHCTailDroppedPkts      Counter64,
    eriRouterQosIfQueueIPv4OutOctets              Counter32,
    eriRouterQosIfQueueIPv4OutPkts                Counter32,
    eriRouterQosIfQueueIPv4WredDroppedOctets      Counter32,
    eriRouterQosIfQueueIPv4WredDroppedPkts        Counter32,
    eriRouterQosIfQueueIPv4TailDroppedOctets      Counter32,
    eriRouterQosIfQueueIPv4TailDroppedPkts        Counter32,
    eriRouterQosIfQueueHCIPv4OutOctets            Counter64,
    eriRouterQosIfQueueHCIPv4OutPkts              Counter64,
    eriRouterQosIfQueueHCIPv4WredDroppedOctets    Counter64,
    eriRouterQosIfQueueHCIPv4WredDroppedPkts      Counter64,
    eriRouterQosIfQueueHCIPv4TailDroppedOctets    Counter64,
    eriRouterQosIfQueueHCIPv4TailDroppedPkts      Counter64,
    eriRouterQosIfQueueIPv6OutOctets              Counter32,
    eriRouterQosIfQueueIPv6OutPkts                Counter32,
    eriRouterQosIfQueueIPv6WredDroppedOctets      Counter32,
    eriRouterQosIfQueueIPv6WredDroppedPkts        Counter32,
    eriRouterQosIfQueueIPv6TailDroppedOctets      Counter32,
    eriRouterQosIfQueueIPv6TailDroppedPkts        Counter32,
    eriRouterQosIfQueueHCIPv6OutOctets            Counter64,
    eriRouterQosIfQueueHCIPv6OutPkts              Counter64,
    eriRouterQosIfQueueHCIPv6WredDroppedOctets    Counter64,
    eriRouterQosIfQueueHCIPv6WredDroppedPkts      Counter64,
    eriRouterQosIfQueueHCIPv6TailDroppedOctets    Counter64,
    eriRouterQosIfQueueHCIPv6TailDroppedPkts      Counter64,
    eriRouterQosIfQueueGreenOutOctets             Counter64,
    eriRouterQosIfQueueGreenOutPkts               Counter64,
    eriRouterQosIfQueueYellowOutOctets            Counter64,
    eriRouterQosIfQueueYellowOutPkts              Counter64,
    eriRouterQosIfQueueRedOutOctets               Counter64,
    eriRouterQosIfQueueRedOutPkts                 Counter64,
    eriRouterQosIfQueueGreenDroppedOctets         Counter64,
    eriRouterQosIfQueueGreenDroppedPkts           Counter64,
    eriRouterQosIfQueueYellowDroppedOctets        Counter64,
    eriRouterQosIfQueueYellowDroppedPkts          Counter64,
    eriRouterQosIfQueueRedDroppedOctets           Counter64,
    eriRouterQosIfQueueRedDroppedPkts             Counter64,
    eriRouterQosIfQueueTotalDroppedOctets         Counter64,
    eriRouterQosIfQueueTotalDroppedPkts           Counter64
}

eriRouterQosIfQueueId OBJECT-TYPE
    SYNTAX      Unsigned32 (0..4294967295)
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
           "A unique identifier for a transmit queue within an
            interface."
    ::= { eriRouterQosInterfaceQueueStatsEntry 1}

eriRouterQosIfQueueOutOctets OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
           "The number of octets transmitted out of this queue.

            Discontinuities in the value of this counter can occur at
            re-initialization of the management system, and at other
            times as indicated by the value of
            ifCounterDiscontinuityTime."
    ::= { eriRouterQosInterfaceQueueStatsEntry 2}

eriRouterQosIfQueueOutPkts OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
           "The number of packets transmitted out of this queue.

            Discontinuities in the value of this counter can occur at
