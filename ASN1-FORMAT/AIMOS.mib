        aimosAlarmListAlarmEventID, aimosAlarmListAlarmID, aimosAlarmListSpecificProblem, aimosAlarmListAdditionalText, aimosAlarmListSDN, 
        aimosAlarmListTrapInfo, aimosAlarmListComponentName, aimosAlarmListLocation, aimosAlarmListUniqueSystemID, aimosAlarmListRecognitionUser, 
        aimosAlarmListRecognitionTime, aimosAcknowledgementOID }
