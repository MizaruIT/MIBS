    AimosBandCalculationEntry ::=
      SEQUENCE { 
        aimosBandCalculationID
          TableIndex,
        aimosBandCalculationMin
          ARFCN,
        aimosBandCalculationMax
          ARFCN,
        aimosBandCalculationFormulaStart
          FrequencyRange,
        aimosBandCalculationFormulaBandWidth
          Integer32,
        aimosBandCalculationFormulaOffset
          ARFCN
       }

    aimosBandCalculationID OBJECT-TYPE
      SYNTAX TableIndex (1..5)
      MAX-ACCESS read-only
      STATUS current
      DESCRIPTION
        "Enumerated consecutively."
      ::= { aimosBandCalculationEntry 1 }

    
    aimosBandCalculationMin OBJECT-TYPE
      SYNTAX ARFCN
      MAX-ACCESS read-only
      STATUS current
      DESCRIPTION
        "First ARFCN for wich the aimosBandCalculationFormula-leafs are valid."
      ::= { aimosBandCalculationEntry 2 }

    
    aimosBandCalculationMax OBJECT-TYPE
      SYNTAX ARFCN
      MAX-ACCESS read-only
      STATUS current
      DESCRIPTION
        "Last ARFCN for wich the aimosBandCalculationFormula-leafs are valid."
      ::= { aimosBandCalculationEntry 3 }

    
    aimosBandCalculationFormulaStart OBJECT-TYPE
      SYNTAX FrequencyRange
      UNITS "kHz"
      MAX-ACCESS read-only
      STATUS current
      DESCRIPTION
        "You can calculate the frequency for any ARFCN with the following formula:
        
        fDL(ARFCN) = aimosBandCalculationFormulaStart
                   + aimosBandCalculationFormulaGap *
                     (ARFCN-aimosBandCalculationFormulaOffset)
        
        Example: formula for Extended GSM 900 is 935000 + 200 * (ARFCN - 1024)
        
        Determine frequency of ARFCN 1023 in E-GSM900:
        
        fDL(1023) = 935.000 + 200 * (1023 - 1024) = 934.800 kHz
        
        In this example aimosBandCalculationFormulaStart must have the value 935000."
      ::= { aimosBandCalculationEntry 4 }

    
    aimosBandCalculationFormulaBandWidth OBJECT-TYPE
      SYNTAX Integer32 (25..2500)
      UNITS "kHz"
      MAX-ACCESS read-only
      STATUS current
      DESCRIPTION
        "You can calculate the frequency for any ARFCN with the following formula:
        
        fDL(ARFCN) = aimosBandCalculationFormulaStart
                   + aimosBandCalculationFormulaGap *
                     (ARFCN-aimosBandCalculationFormulaOffset)
        
        Example: formula for Extended GSM 900 is 935000 + 200 * (ARFCN - 1024)
        
        Determine frequency of ARFCN 1023 in E-GSM900:
        
        fDL(1023) = 935.000 + 200 * (1023 - 1024) = 934.800 kHz
        
        In this example aimosBandCalculationFormulaGap must have the value 200."
      ::= { aimosBandCalculationEntry 5 }

    
    aimosBandCalculationFormulaOffset OBJECT-TYPE
      SYNTAX ARFCN
      MAX-ACCESS read-only
      STATUS current
      DESCRIPTION
        "You can calculate the frequency for any ARFCN with the following formula:
        
        fDL(ARFCN) = aimosBandCalculationFormulaStart
                   + aimosBandCalculationFormulaGap *
                     (ARFCN-aimosBandCalculationFormulaOffset)
        
        Example: formula for Extended GSM 900 is 935000 + 200 * (ARFCN - 1024)
        
        Determine frequency of ARFCN 1023 in E-GSM900:
        
        fDL(1023) = 935.000 + 200 * (1023 - 1024) = 934.800 kHz
        
        In this example aimosBandCalculationFormulaOffset must have the value 1024."
      ::= { aimosBandCalculationEntry 6 }

    
