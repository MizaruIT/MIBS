IShelfCardSoftwareRevision ::= TEXTUAL-CONVENTION
    -- %CLICOLLABEL SoftwareRev
    -- %CLIWIDTH 14
    DISPLAY-HINT "14a"
    STATUS      current
    DESCRIPTION "Format MM.mm.PPbuild where MM is the major number,
                 mm is the minor number, PP is the patch number and
                 build is a build notation. The numbers are decimal
                 with leading 0, e.g. 01 for 1. The build notation can
                 be non-numeric. If the string is shorter than the full
                 14 characters the remainder will be padded with blanks."
    SYNTAX      OCTET STRING  (SIZE(14))

IShelfDateTime ::= TEXTUAL-CONVENTION
    -- %CLICOLLABEL DateTime
    DISPLAY-HINT "2d-1d-1d,1d:1d:1d"
    STATUS       current
    DESCRIPTION
            "A date-time specification.

            field  octets  contents                  range
            -----  ------  --------                  -----
              1      1-2   year*                     0..65536
              2       3    month                     1..12
              3       4    day                       1..31
              4       5    hour                      0..23
              5       6    minutes                   0..59
              6       7    seconds                   0..59

            For example, Tuesday May 26, 1992 at 1:30:15 PM EDT would be
            displayed as:

                             1992-5-26,13:30:15"
    SYNTAX       OCTET STRING (SIZE (7))

