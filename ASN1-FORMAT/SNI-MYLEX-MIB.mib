------------------------------------------------------------------------------------

mylexAdapterHostTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF MylexAdapterHost
    ACCESS      not-accessible
    STATUS      mandatory
    DESCRIPTION "Mylex adapter host information table.
                 Only for SCSI-to-SCSI adapters (DAC960SX).
                 Defines for each Mylex adapter to which host controller,
                 bus and target the adapter is connected to."
    ::= { sniMlxDiskArray 2 }

mylexAdapterHost OBJECT-TYPE
    SYNTAX      MylexAdapterHost
    ACCESS      not-accessible
    STATUS      mandatory
    DESCRIPTION ""
    INDEX { mylexAdapterNum }
    ::= { mylexAdapterHostTable 1 }


MylexAdapterHost ::= SEQUENCE
    {
    mylexAdapterNum
        INTEGER,
    mylexAdapterHostTargetId
        INTEGER,
    mylexAdapterHostChannelNumber
        INTEGER,
    mylexAdapterHostControllerNumber
        INTEGER
        }


mylexAdapterNum OBJECT-TYPE
    SYNTAX      INTEGER
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION "Adapter number (index)"
    ::= { mylexAdapterHost 1 }

mylexAdapterHostTargetId  OBJECT-TYPE
    SYNTAX      INTEGER
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION "Target ID of the Mylex SCSI-to-SCSI Adapter.
                 Will be 0xFFFF for local adapters."
    ::= { mylexAdapterHost 2 }

mylexAdapterHostChannelNumber  OBJECT-TYPE
    SYNTAX      INTEGER
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION "SCSI host channel number the Mylex SCSI-to-SCSI Adapter
                 is connected to; will be 0 for local adapters."
    ::= { mylexAdapterHost 3 }

mylexAdapterHostControllerNumber  OBJECT-TYPE
    SYNTAX      INTEGER
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION "SCSI host controller number the Mylex SCSI-to-SCSI Adapter
                 is connected to; will be 0 for local adapters."
    ::= { mylexAdapterHost 4 }

------------------------------------------------------------------------------------

mylexAdapterInfoTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF MylexAdapterInfoEntry
    ACCESS      not-accessible
    STATUS      mandatory
    DESCRIPTION "Mylex Adapter Info table"
    ::= { sniMlxDiskArray 3 }

mylexAdapterInfoEntry OBJECT-TYPE
    SYNTAX      MylexAdapterInfoEntry
    ACCESS      not-accessible
    STATUS      mandatory
    DESCRIPTION "Mylex Adapter Info table entry"
    INDEX { mylexAdapterInfoId }
    ::= { mylexAdapterInfoTable 1 }


MylexAdapterInfoEntry ::= SEQUENCE
    {
    mylexAdapterInfoId          INTEGER,
    mylexModel                  DisplayString,
    mylexCacheSize              INTEGER,
    mylexBaseAddress            INTEGER,
    mylexBusType                INTEGER,
    mylexInterruptIRQ           INTEGER,
    mylexFirmwareVersion        DisplayString,
    mylexBIOSVersion            DisplayString,
    mylexDefaultRebuildRate     INTEGER,
    mylexNumChannels            Gauge,
    mylexMaxChannels            INTEGER,
    mylexNumLogicalDrives       Gauge,
    mylexMaxLogicalDrives       INTEGER,
    mylexNumPhysicalDevices     Gauge,
    mylexMaxPhysicalDevices     INTEGER,
    mylexStripeSize             INTEGER,
    mylexFlashEEpromSize        INTEGER,
    mylexPhysicalSectorSize     INTEGER,
    mylexLogicalSectorSize      INTEGER,
    mylexSlotNumber             INTEGER,
    mylexVendorName             DisplayString
    }


mylexAdapterInfoId OBJECT-TYPE
    SYNTAX      INTEGER
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION "Adapter number (index)"
    ::= { mylexAdapterInfoEntry 1 }

mylexModel      OBJECT-TYPE
    SYNTAX      DisplayString
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION "Adapter model/submodel"
    ::= { mylexAdapterInfoEntry 2 }

mylexCacheSize  OBJECT-TYPE
    SYNTAX      INTEGER
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION "Size of cache RAM module (kBytes); -1 if unknown"
    ::= { mylexAdapterInfoEntry 3 }

mylexBaseAddress OBJECT-TYPE
    SYNTAX      INTEGER
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION "I/O base address (-1 if unknown)"
    ::= { mylexAdapterInfoEntry 4 }

mylexBusType    OBJECT-TYPE
    SYNTAX      INTEGER
        {
        isa     (1),
        eisa    (2),
        mcas    (3),
        turbo   (4),
        pci     (5),
        scsi    (6),
        other   (99)
        }
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION "Bus type (EISA/MCAS/PCI ...) "
    ::= { mylexAdapterInfoEntry 9 }

mylexInterruptIRQ OBJECT-TYPE
    SYNTAX      INTEGER
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION "Interrupt request line (IRQ); -1 if unknown"
    ::= { mylexAdapterInfoEntry 10 }

mylexFirmwareVersion OBJECT-TYPE
    SYNTAX      DisplayString
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION "Firmware version"
    ::= { mylexAdapterInfoEntry 11 }

mylexBIOSVersion OBJECT-TYPE
    SYNTAX      DisplayString
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION "BIOS version"
    ::= { mylexAdapterInfoEntry 12 }

mylexDefaultRebuildRate OBJECT-TYPE
    SYNTAX      INTEGER
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION "Default rebuild/check rate (%)"
    ::= { mylexAdapterInfoEntry 13 }

mylexNumChannels OBJECT-TYPE
    SYNTAX      Gauge
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION "Current number of channels"
    ::= { mylexAdapterInfoEntry 14 }

mylexMaxChannels OBJECT-TYPE
    SYNTAX      INTEGER
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION "Maximum number of channels"
    ::= { mylexAdapterInfoEntry 15 }

mylexNumLogicalDrives OBJECT-TYPE
    SYNTAX      Gauge
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION "Current number of logical drives "
    ::= { mylexAdapterInfoEntry 16 }

mylexMaxLogicalDrives OBJECT-TYPE
    SYNTAX      INTEGER
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION "Maximum logical drives"
    ::= { mylexAdapterInfoEntry 17 }

mylexNumPhysicalDevices OBJECT-TYPE
    SYNTAX      Gauge
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION "Current number of physical devices"
    ::= { mylexAdapterInfoEntry 18 }

mylexMaxPhysicalDevices OBJECT-TYPE
    SYNTAX      INTEGER
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION "Maximum physical devices"
    ::= { mylexAdapterInfoEntry 19 }

mylexStripeSize OBJECT-TYPE
    SYNTAX      INTEGER
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION "Stripe Size (8K chunks)"
    ::= { mylexAdapterInfoEntry 20 }

mylexFlashEEpromSize OBJECT-TYPE
    SYNTAX      INTEGER
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION "Size of flash EEPROM (kBytes)"
    ::= { mylexAdapterInfoEntry 21 }

mylexPhysicalSectorSize OBJECT-TYPE
    SYNTAX      INTEGER
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION "Size of a physical sector (Bytes)"
    ::= { mylexAdapterInfoEntry 22 }

mylexLogicalSectorSize OBJECT-TYPE
    SYNTAX      INTEGER
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION "Size of logical sector (Bytes)"
    ::= { mylexAdapterInfoEntry 23 }

mylexSlotNumber OBJECT-TYPE
    SYNTAX      INTEGER
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION "Slot number the adapter is in"
    ::= { mylexAdapterInfoEntry 24 }

mylexVendorName OBJECT-TYPE
    SYNTAX      DisplayString
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION "Name of the adapter vendor"
    ::= { mylexAdapterInfoEntry 25 }

