    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION "Channel number (index)"
    ::= { mylexPhysicalDeviceInfoEntry 2 }

mylexPhysicalDeviceDevNr      OBJECT-TYPE
    SYNTAX      INTEGER
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION "Physical device number (index)"
    ::= { mylexPhysicalDeviceInfoEntry 3 }

mylexPhysicalDeviceModel      OBJECT-TYPE
    SYNTAX      DisplayString
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION "Physical device model name"
    ::= { mylexPhysicalDeviceInfoEntry 5 }

mylexPhysicalDeviceCapacity OBJECT-TYPE
    SYNTAX      INTEGER
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION "Disk capacity (MBytes; only for configured DAC disks)"
    ::= { mylexPhysicalDeviceInfoEntry 6 }

mylexPhysicalDeviceStatus OBJECT-TYPE
    SYNTAX      INTEGER
        {
        dead    (1),
        online  (2),
        standby (3),
        rebuild (4),
        unknown (9)
        }
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION "Disk status (only for configured DAC disks)"
    ::= { mylexPhysicalDeviceInfoEntry 7 }

mylexPhysicalDeviceRebuildRate OBJECT-TYPE
    SYNTAX      INTEGER
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION "Rebuild rate (only for configured DAC disks)"
    ::= { mylexPhysicalDeviceInfoEntry 8 }

mylexPhysicalDeviceParityErrors OBJECT-TYPE
    SYNTAX      Counter
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION "Parity error count (only for configured DAC disks)"
    ::= { mylexPhysicalDeviceInfoEntry 9 }

mylexPhysicalDeviceSoftErrors OBJECT-TYPE
    SYNTAX      Counter
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION "Soft error count (only for configured DAC disks)"
    ::= { mylexPhysicalDeviceInfoEntry 10 }

mylexPhysicalDeviceHardErrors OBJECT-TYPE
    SYNTAX      Counter
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION "Hardware error count (only for configured DAC disks)"
    ::= { mylexPhysicalDeviceInfoEntry 11 }

mylexPhysicalDeviceMiscErrors OBJECT-TYPE
    SYNTAX      Counter
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION "Miscellaneous error count (only for configured DAC disks)"
    ::= { mylexPhysicalDeviceInfoEntry 12 }

mylexPhysicalDeviceDiskConfigured OBJECT-TYPE
    SYNTAX      INTEGER
        {
        true    (1),
        false   (2)
        }
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION "Device is a configured DAC disk"
    ::= { mylexPhysicalDeviceInfoEntry 13 }

mylexPhysicalDeviceScsiDeviceType OBJECT-TYPE
    SYNTAX      INTEGER
        {
        disk        (1),
        tape        (2),
        printer     (3),
        processor   (4),
        writeOnce   (5),
        cdRom       (6),
        scanner     (7),
        optical     (8),
        jukebox     (9),
        commDev     (10),
        host        (98),
        unknown     (99)
        }
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION "SCSI device type"
    ::= { mylexPhysicalDeviceInfoEntry 14 }

mylexPhysicalDeviceParamFastScsi OBJECT-TYPE
    SYNTAX      INTEGER
        {
        true    (1),
        false   (2)
        }
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION "SCSI device parameter: Fast SCSI"
    ::= { mylexPhysicalDeviceInfoEntry 15 }

mylexPhysicalDeviceParamWideScsi OBJECT-TYPE
    SYNTAX      INTEGER
        {
        true    (1),
        false   (2)
        }
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION "SCSI device parameter: Wide SCSI"
    ::= { mylexPhysicalDeviceInfoEntry 16 }

mylexPhysicalDeviceParamTaggedQueueing OBJECT-TYPE
    SYNTAX      INTEGER
        {
        true    (1),
        false   (2)
        }
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION "SCSI device parameter: Tagged queueing"
    ::= { mylexPhysicalDeviceInfoEntry 17 }


mylexPhysicalDeviceSmartCompliant OBJECT-TYPE
    SYNTAX  INTEGER {
        true(1),
        false(2)
        }
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "Is this device SMART (Self Monitoring Analysis and Reporting Technology)
         compliant? If SMART information cannot be obtained, false(2) must be reported."
    ::= { mylexPhysicalDeviceInfoEntry 18 }

mylexPhysicalDeviceSmartMonitoring OBJECT-TYPE
    SYNTAX  INTEGER {
        enable(1),
        disable(2),
        not-available(3)
        }
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
        "In write mode, only enable and disable are allowed.
        An error will be reported if you try to enable a non Smart
        compliant device.
        In read mode, not-available(3) is returned if the device is not Smart
        compliant. Disable will be reported, if one option or more
        has changed after the initial configuration of the agent."
    ::= { mylexPhysicalDeviceInfoEntry 19 }

mylexPhysicalDeviceSmartStatus OBJECT-TYPE
    SYNTAX  INTEGER {
        ok(1),
        failure-predicted(2),
        not-available(3)
        }
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "If not SmartCompliant or SmartMonitoring disabled,
        not-available(3) must be reported."
    ::= { mylexPhysicalDeviceInfoEntry 20 }

