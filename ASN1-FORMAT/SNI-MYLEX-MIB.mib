    mylexPhysicalDeviceSmartMonitoring      INTEGER,
    mylexPhysicalDeviceSmartStatus          INTEGER
    }


mylexPhysicalDeviceAdapterId     OBJECT-TYPE
    SYNTAX      INTEGER
    ACCESS      read-only
    STATUS      mandatory
