sniMylexAgentInfo   OBJECT IDENTIFIER       ::= { sniMlxDiskArray 1 }


mylexAgentId    OBJECT-TYPE
    SYNTAX      DisplayString
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION "Mylex SNMP agent ID string"
    ::= { sniMylexAgentInfo 1 }

mylexAgentCompany   OBJECT-TYPE
    SYNTAX      DisplayString
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION "Mylex SNMP agent company string"
    ::= { sniMylexAgentInfo 2 }

mylexAgentVersion   OBJECT-TYPE
    SYNTAX      DisplayString
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION "Mylex SNMP agent version string"
    ::= { sniMylexAgentInfo 3 }

mylexAgentBuild   OBJECT-TYPE
    SYNTAX      DisplayString
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION "Mylex SNMP agent build date and time string"
    ::= { sniMylexAgentInfo 4 }

mylexAgentVersionMajor   OBJECT-TYPE
    SYNTAX      INTEGER
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION "Mylex SNMP agent major version number"
