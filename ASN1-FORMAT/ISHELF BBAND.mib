-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
--
-- iShelfBroadBandSppTable - Broadband Service Provider Profile
--
-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++

-- %CLICMDPREFIX broadband
-- %CLICMDABBR spp
-- %CLISHOWGROUP spp
iShelfBroadBandSppTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IShelfBroadBandSppEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This table identifies the list of configured Service Provider
         Profiles in the system.  This read-only table is auto-created/deleted
         when rows are added/deleted from the specific SPP tables that follow.
         The FacilityType field will indicate which of the specific SPP tables
         a given row will be found in."
    ::= { iShelfBroadBand 1 }

iShelfBroadBandSppEntry OBJECT-TYPE
    SYNTAX      IShelfBroadBandSppEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION ""
    INDEX       { iShelfBroadBandSppIndex }
    ::= { iShelfBroadBandSppTable 1 }

IShelfBroadBandSppEntry ::=
    SEQUENCE {
        iShelfBroadBandSppIndex
            IShelfSppIndex,
        iShelfBroadBandSppFacilityType
            IShelfSppFacilityType,
        iShelfBroadBandSppLocation
            IShelfLocation,
        iShelfBroadBandSppCircuit
            IShelfCircuit
    }

-- %CLISHOWGROUP spp
-- %CLICOLLABEL Idx
iShelfBroadBandSppIndex OBJECT-TYPE
    SYNTAX      IShelfSppIndex
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "This identifies the SPP."
    ::= { iShelfBroadBandSppEntry 1 }

-- %CLISHOWGROUP spp
-- %CLICOLLABEL Type
iShelfBroadBandSppFacilityType OBJECT-TYPE
    SYNTAX      IShelfSppFacilityType
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "The type of facilities used by the service provider."
    ::= { iShelfBroadBandSppEntry 2 }

-- %CLISHOWGROUP spp
-- %CLICOLLABEL Location
iShelfBroadBandSppLocation OBJECT-TYPE
    SYNTAX      IShelfLocation
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The base location of the SPP.
         This object must be consistent with the facility type object.
         For DS1 Facilities this is the Ethernet card location.
         For DS1-IMA Facilities this is the B3 card location.
         For Sonet Facilities this is the OC-3, OC-12 or OC-48 card location.
         For Ethernet Facilities this is the Ethernet card location.
         For GigEthernet Facilities this is the GigEthernet card location.
         For OltGigEthernet Facilities this is the OLT card location."
    ::= { iShelfBroadBandSppEntry 3 }

-- %CLISHOWGROUP spp
-- %CLICOLLABEL Circuit
iShelfBroadBandSppCircuit OBJECT-TYPE
    SYNTAX      IShelfCircuit
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The base circuit for the SPP."
    ::= { iShelfBroadBandSppEntry 4 }

