--
-- iShelfBroadBandSppDs3Table - Broadband SPP for Ds3 Facility
--
-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++

-- %CLICMDPREFIX broadband
-- %CLICMDABBR ds3spp
-- %CLISHOWGROUP ds3spp
iShelfBroadBandSppDs3Table OBJECT-TYPE
    SYNTAX      SEQUENCE OF IShelfBroadBandSppDs3Entry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Definition of a DS3 SPP.
         Note that the DS3 SPP can be configured with 1 - 16 VPIs (1 primary and up to 15 extra).
         There is a total DS3 SPP budget of 2000 VCs that can be allocated any way across the 16 VPIs.
         For example, a single VPI can be allocated all 2000 VCs or 5 VPIs can each be allocated 400 VCs."
    ::= { iShelfBroadBand 14 }

iShelfBroadBandSppDs3Entry OBJECT-TYPE
    SYNTAX      IShelfBroadBandSppDs3Entry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "Each entry is a separate DS3 SPP uplink."
    INDEX       { iShelfBroadBandSppDs3Index }
    ::= { iShelfBroadBandSppDs3Table 1 }

IShelfBroadBandSppDs3Entry ::=
    SEQUENCE {
        iShelfBroadBandSppDs3Index
            IShelfSppIndex,
        iShelfBroadBandSppDs3Location
            IShelfLocation,
        iShelfBroadBandSppDs3Label
            IShelfBroadbandLabel,
        iShelfBroadBandSppDs3DefaultVpi
            IShelfVpi,
