    INDEX {
        etsysIpSlaConfigIndex,
        etsysIpSlaCollectionType,
        etsysIpSlaCollectionIndex,
        etsysIpSlaPathIndex,
        etsysIpSlaHopIndex,
        etsysIpSlaBucketIndex
