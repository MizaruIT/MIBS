    ::= { etsysIpSlaRttDataEntry 9 }

etsysIpSlaRttDataPktOutOfOrder OBJECT-TYPE
    SYNTAX       Gauge32
    UNITS        "packets"
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION
