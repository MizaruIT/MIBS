    			rms-cTemp2Name,
    			rms-cTemp2LowerWarningLevel
    			}
    DESCRIPTION
        "Temperature is below the low warning limit."
	--#TYPE		"Temperature 2 below lower warning limit"
	--#SUMMARY	"Temperature %s below %d in %s at %s"
	--#ARGUMENTS	{2,3,0,1}
	--#SEVERITY 	MINOR
	--#TIMEINDEX	4
	--#STATE		NONOPERATIONAL
    ::= 122

