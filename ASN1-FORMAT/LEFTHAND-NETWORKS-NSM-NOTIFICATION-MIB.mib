lhnNsmNotificationGeneric NOTIFICATION-TYPE
	OBJECTS {           
					message,
					messageTime,
					eventID,
					severity,
					hostname,
					primaryIP,
					mac,
					serialNumber,
					modelName,
					productName,
					productID,
					hpim,
					softwareVersion,
					managementGroupVersion,
					managementGroupSerialNumber,
					managementGroup,
					cluster,
					site
			}
	STATUS              current
	DESCRIPTION         "A generic system event was generated.
						The user chose to be notified of this condition
						via SNMP. A table of messages exists which stores
						all the notifications issued. This trap will
						contain the OID referring to the corresponding
						message stored in this table.
						Added in SAN/iQ 8.5"
	::= { lhnNsmEvents 1 }

