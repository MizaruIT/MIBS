	DESCRIPTION
		"Write access is not allowed."

	VARIATION	sonetMediumLoopbackConfig
	ACCESS		read-only
	DESCRIPTION
		"Write access is not allowed."

	VARIATION	sonetSESthresholdSet
	ACCESS		read-only
	DESCRIPTION
		"Write access is not allowed."

	VARIATION	sonetPathCurrentWidth
	ACCESS		read-only
	DESCRIPTION
		"Write access is not allowed."

        ::= { rbnIetfSonetCap 1  }	

