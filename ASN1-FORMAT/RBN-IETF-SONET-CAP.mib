RBN-IETF-SONET-CAP DEFINITIONS ::= BEGIN

IMPORTS
	MODULE-IDENTITY
		FROM SNMPv2-SMI
	AGENT-CAPABILITIES
		FROM SNMPv2-CONF
	rbnCapabilities
		FROM RBN-SMI;

rbnIetfSonetCap MODULE-IDENTITY
        LAST-UPDATED    "201106150000Z" -- Jun 15, 2011
        ORGANIZATION    "Ericsson Inc."
        CONTACT-INFO
                "           Ericsson, Inc. 

                            100 Headquarters Dr
                            San Jose, CA  95134
                            USA

                     Phone: +1 408 750 5000
                       Fax: +1 408 750 5599
                "
        DESCRIPTION
		"The Agent Capabilities of the SONET-MIB.

                If this MIB implementation is platform-dependent,
                the PRODUCT-RELEASE clause contains Ericsson SEOS version and
                at least one of the following Ericsson product families:
                    [SE]  SmartEdge Multi-service Edge Router (MSER) family
                    [SM]  SM family of Metro Ethernet Service Transport platforms
                    [SSR] Smart Service Router (SSR) family"

        REVISION        "201106150000Z" -- Jun 15, 2011
        DESCRIPTION
            "Update DESCRIPTION clause of MODULE-IDENTITY
            and PRODUCT-RELEASE clause of AGENT-CAPABILITIES."
        REVISION    "201011010000Z" -- Nov 1, 2010
        DESCRIPTION
            "Modified PRODUCT-RELEASE clause for consistent naming.
             Updated CONTACT-INFO."

	REVISION	"200405280000Z" -- May 28, 2004
	DESCRIPTION
		"Added new AGENT-CAPABILITIES for the SE 2.6.5 release."

	REVISION	"200110160000Z" -- Oct 16, 2001
	DESCRIPTION
		"Added SmartEdge capabilities.  Updated CONTACT-INFO."

	REVISION	"199902251000Z" -- February 25, 1999
	DESCRIPTION
		"The original revision of this module."
	::= { rbnCapabilities 19 }

rbnIetfSonetCap1 AGENT-CAPABILITIES
	PRODUCT-RELEASE	"AOS 3.0.X.X"
	STATUS		current
	DESCRIPTION	"SONET-MIB capabilities"
	SUPPORTS	SONET-MIB
	INCLUDES {
		sonetMediumStuff2,
		sonetSectionStuff2,
		sonetLineStuff2,
		sonetPathStuff2,
		sonetFarEndLineStuff2,
		sonetFarEndPathStuff2
		}


	VARIATION	sonetMediumType
	ACCESS		read-only
	DESCRIPTION
		"Write access is not allowed."
                
	VARIATION	sonetMediumLineCoding
	ACCESS		read-only
	DESCRIPTION
		"Write access is not allowed."

	VARIATION	sonetMediumLineType
	ACCESS		read-only
	DESCRIPTION
		"Write access is not allowed."

	VARIATION	sonetMediumCircuitIdentifier
