-- for DeepGet() 
-- %CLICMDABBR status
-- %CLICOLLABEL status
-- %CLIWIDTH 8
-- %CLISHOWGROUP map.1
iShelfNodeMapLinkStatus OBJECT-TYPE
    SYNTAX      INTEGER {
                    unknown(0),
                    normal(1),
                    fault(2),
                    dryUp(3),
                    noComm(4)
                }
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "This object indicates the current state of the link
         supporting the node map.  The following values are supported:
             unknown - no communication to CPU
             normal - the link is operational
             fault - link has a problem (i.e., missing card, LOS etc.)
             dryUp - link has been placed out of service
             noComm - link status cannot be determined"
    ::= { iShelfNodeMapEntry 12 }

-- for DeepGet() 
-- %CLICMDABBR usRedun
-- %CLICOLLABEL usRedun
-- %CLIWIDTH 8
-- %CLISHOWGROUP map.2
iShelfNodeMapUsRedundancyStatus OBJECT-TYPE
    SYNTAX      INTEGER {
                    none(0),
                    stbyProt(1),
                    stbyWrk(2),
                    actProt(3),
                    actWrk(4),
		    unknown(5)
                }
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "This object indicates the current redundancy state of
         the link.  This is only applicable for OC12 type links.

         The following status are possible:
             none - not applicable or not accessible
             stbyProt - upstream is standby card and protect card
             stbyWrk - upstream is standby card and working card
             actProt - upstream is active card and protect card
             actWrk - upstream is active card and working card"
    ::= { iShelfNodeMapEntry 13 }

-- for DeepGet() 
-- %CLICMDABBR dsRedun
-- %CLICOLLABEL dsRedun
