-- iShelfSysFileTransferTable - 
--
-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++

-- %CLICMDABBR ftt
-- %CLISHOWGROUP ftt
iShelfSysFileTransferTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IShelfSysFileTransferEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This table provides the ability to download a file to the system
         or upload a file from the system. It provides a generic way of
         initiating either a software download (software upgrade) or a
         database backup/restore (database upload/download). Only a single
         entry can be created in this table."
    ::= { iShelfSysGroup 11 }

iShelfSysFileTransferEntry OBJECT-TYPE
    SYNTAX      IShelfSysFileTransferEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "One row is for software download and one row is for database backup/restore."
    INDEX       { iShelfSysFileTransferIndex }
    ::= { iShelfSysFileTransferTable 1 }

IShelfSysFileTransferEntry ::=
    SEQUENCE {
        iShelfSysFileTransferIndex
            INTEGER,
        iShelfSysFileTransferServerIpAddress
            IpAddress,
        iShelfSysFileTransferPort
            INTEGER,
        iShelfSysFileTransferFilename
            IShelfDownloadFilename,
	iShelfSysFileTransferCardLocation
	    IShelfLocation,
        iShelfSysFileTransferAction
            INTEGER,
        iShelfSysFileTransferProgress
            INTEGER,
        iShelfSysFileTransferOperState
            INTEGER,
        iShelfSysFileTransferOperStateDescription
            IShelfGenericString,
        iShelfSysFileTransferRowStatus
            RowStatus
    }

-- %CLICOLLABEL Index
-- %CLIWIDTH 10
-- %CLISHOWGROUP ftt.1
-- %CLISHOWGROUP ftt.2
-- %CLISHOWGROUP ftt.3
-- %CLISHOWGROUP ftt.4
iShelfSysFileTransferIndex OBJECT-TYPE
    SYNTAX      INTEGER {
                    softwareDownload(1),
                    databaseBackupRestore(2)
                }
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The first entry defines software download attributes and the second
         entry defines database backup and restore attributes."
    ::= { iShelfSysFileTransferEntry 1 }

-- %CLICOLLABEL IPAddr
-- %CLISHOWGROUP ftt.1
iShelfSysFileTransferServerIpAddress OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION "The TFTP or FTP server that will handle the file transfer."
    ::= { iShelfSysFileTransferEntry 2 }

-- %CLICOLLABEL Port
-- %CLISHOWGROUP ftt.1
iShelfSysFileTransferPort OBJECT-TYPE
    SYNTAX      INTEGER (0..65535)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION "The TCP/UDP port to use for the file transfer."
    ::= { iShelfSysFileTransferEntry 3 }

-- %CLICOLLABEL Filename
-- %CLISHOWGROUP ftt.2
iShelfSysFileTransferFilename OBJECT-TYPE
    SYNTAX      IShelfDownloadFilename
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION "The file that will be downloaded or uploaded."
    ::= { iShelfSysFileTransferEntry 4 }

-- %CLICOLLABEL Location
-- %CLISHOWGROUP ftt.1
iShelfSysFileTransferCardLocation OBJECT-TYPE
    SYNTAX      IShelfLocation
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "The physical address location of the card to download.
         This location will specify node, shelf, and slot.
