--
-- iShelfSysAlarmSettingsTable - 
--
-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++

-- %CLICMDPREFIX sys
-- %CLICMDABBR alarmsettings
iShelfSysAlarmSettingsTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IShelfSysAlarmSettingsEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "The Alarm Settings table provides the ability for the operator to
         override the internal default settings for each alarm. For example, assuming the
         ONT Missing alarm is MINOR by default, with this table a row can be created for the ONT
         Missing alarm and the severity level can be changed to MAJOR."
    ::= { iShelfSysGroup 7 }

iShelfSysAlarmSettingsEntry OBJECT-TYPE
    SYNTAX      IShelfSysAlarmSettingsEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "Each entry corresponds to one Alarm."
    INDEX       { iShelfSysAlarmSettingsId }
    ::= {iShelfSysAlarmSettingsTable 1 }

IShelfSysAlarmSettingsEntry ::=
    SEQUENCE {
        iShelfSysAlarmSettingsId
            IShelfAlarmId,
        iShelfSysAlarmSettingsName
            IShelfAlarmName,
        iShelfSysAlarmSettingsLevel
            IShelfAlarmLevel,
        iShelfSysAlarmSettingsState
            IShelfAlarmState,
        iShelfSysAlarmSettingsRowStatus
            RowStatus
    }

-- %CLIWIDTH 20
iShelfSysAlarmSettingsId OBJECT-TYPE
    SYNTAX      IShelfAlarmId
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The type of alarm."
    ::= { iShelfSysAlarmSettingsEntry 1 }

-- %SETINACTIVE
-- %CLIOPTION
iShelfSysAlarmSettingsName OBJECT-TYPE
    SYNTAX      IShelfAlarmName
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "User name for the alarm.  This object can only set for
         the usrX alarm ids. All other alarms have a predefined system name that
         cannot be changed.
         NOTE:  This object can only be modified with a non-active rowStatus."
    ::= { iShelfSysAlarmSettingsEntry 2 }

-- %SETINACTIVE
iShelfSysAlarmSettingsLevel OBJECT-TYPE
    SYNTAX      IShelfAlarmLevel
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "The severity level of the alarm.
         critical(1),
         major(2),
         minor(3),
         warning(4),
         event(5),
         clear(6)
        NOTE:  This object can only be modified with a non-active rowStatus."
    ::= { iShelfSysAlarmSettingsEntry 3 }

-- %SETINACTIVE
iShelfSysAlarmSettingsState OBJECT-TYPE
    SYNTAX      IShelfAlarmState
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Whether the alarm is enabled or disabled. By default, all alarms are
         enabled. Disabling an alarm means there will be no user notification (no alarm) if
         the underlying condition is detected.
        NOTE:  This object can only be modified with a non-active rowStatus."
    ::= { iShelfSysAlarmSettingsEntry 4 }

-- %ROWSTATUSTYPE FIXED
-- %CLICOLLABEL RowStatus
-- %CLIWIDTH 12
iShelfSysAlarmSettingsRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This object is used to modify the status of the
         associated row.  Settable objects within the row can only
         be modified when the row is in the notInService state.
         The notInService setting has no impact on the underlying
         Object but is used to cause atomic updates of the row
         once it is move to the active state.

         The supported values of RowStatus are:
            active (1)
            notInService (2)"
    ::= { iShelfSysAlarmSettingsEntry 5 }

