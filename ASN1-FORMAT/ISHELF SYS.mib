        iShelfSysMgmtIfAdminState
            IShelfAdminState,
        iShelfSysMgmtIfOperState
            IShelfOperState,
        iShelfSysMgmtIfAllowAddress
            IpAddress,
        iShelfSysMgmtIfAllowMask
            IpAddress,
        iShelfSysMgmtIfActualAddress
            IpAddress,
        iShelfSysMgmtIfRowStatus
            RowStatus
    }

-- %CLICOLLABEL Index
-- %CLISHOWGROUP mgmtif.1
-- %CLISHOWGROUP mgmtif.2
-- %CLIWIDTH 9
iShelfSysMgmtIfIndex OBJECT-TYPE
    SYNTAX      INTEGER {
                    snmpTrap1(4),
                    snmpTrap2(5)
                }
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION ""
    ::= { iShelfSysMgmtIfEntry 1 }

-- %SETINACTIVE
-- %CLISHOWGROUP mgmtif.1
-- %CLICOLLABEL Label
iShelfSysMgmtIfLabel OBJECT-TYPE
    SYNTAX      IShelfLabel
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION ""
    ::= { iShelfSysMgmtIfEntry 2 }

-- %SETINACTIVE
-- %CLICOLLABEL Admin
-- %CLISHOWGROUP mgmtif.1
iShelfSysMgmtIfAdminState OBJECT-TYPE
    SYNTAX      IShelfAdminState
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This object is used for locking a given management
         interface.

         In the case of a craft or snmp-mgmt interface, locking the
         interface results in the interface becoming read-only.
         Modifications are not allowed from a locked interface but
         Get operations will be allowed.

         In the case of an snmp-trap type interface locking results
         in no traps being sent to the interface."
    ::= { iShelfSysMgmtIfEntry 3 }

-- %CLICOLLABEL Oper
-- %CLISHOWGROUP mgmtif.1
iShelfSysMgmtIfOperState OBJECT-TYPE
    SYNTAX      IShelfOperState
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "This object indicates the current status of the interface."
    ::= { iShelfSysMgmtIfEntry 4 }

-- %SETINACTIVE
-- %CLICOLLABEL AllowAddr
-- %CLISHOWGROUP mgmtif.2
iShelfSysMgmtIfAllowAddress OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This object indicates the client that is allowed to
         use the interface.  It is not applicable (0.0.0.0) for craft
         type interfaces.

         For snmp-mgmt interfaces:
         - 0.0.0.0/AdminState=unlocked:  Allow SNMP EMS connect from any
           client.
         - 0.0.0.0/AdminState=locked:  Disallow connections on this interface.
         - x.x.x.x:  Allow SNMP EMS connections from clients using the
             indicated address with the AllowMask object.  The address may
             be a subnet rather than a specific host.

         For snmp-trap interfaces:
         - 0.0.0.0 - Interface is unused.
         - x.x.x.x - Send traps to specified host.  This must be a 
                     host address."
    ::= { iShelfSysMgmtIfEntry 5 }

-- %SETINACTIVE
-- %CLICOLLABEL AllowMask
-- %CLISHOWGROUP mgmtif.2
iShelfSysMgmtIfAllowMask OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This object indicates the range of allowed addresses.
         For example for a single host the AllowMask would be 255.255.255.255.
         This object is not applicable for craft type interfaces.
        This object is assumed to be 255.255.255.255 for snmp-trap type
        interfaces."
    ::= { iShelfSysMgmtIfEntry 6 }

-- %CLICOLLABEL ActualAddr
-- %CLISHOWGROUP mgmtif.2
iShelfSysMgmtIfActualAddress OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "This object indicates the actual client connection
         for the interface when the interface is operational."
    ::= { iShelfSysMgmtIfEntry 7 }

-- %ROWSTATUSTYPE FIXED
-- %CLICOLLABEL RowStatus
-- %CLIWIDTH 12
-- %CLISHOWGROUP mgmtif.2
iShelfSysMgmtIfRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This object is used to modify the status of the
         associated row.  Settable objects within the row can only
         be modified when the row is in the notInService state.
         The notInService setting has no impact on the underlying
         Object but is used to cause atomic updates of the row
         once it is move to the active state.

         The supported values of RowStatus are:
            active (1)
            notInService (2)"
    ::= { iShelfSysMgmtIfEntry 8 }

