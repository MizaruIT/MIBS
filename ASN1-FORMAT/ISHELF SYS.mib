                    swDnldFailedTxInProgress(3),
                    swDnldFailedServerUnreachable(4),
                    swDnldFailedNetworkTimeout(5),
                    swDnldFailedFileCorruption(6),
                    swDnldFailedInternal(7),
                    dbBakInProgress(8),
                    dbBakCompleteSuccess(9),
                    dbBakFailedTxInProgress(10),
                    dbBakFailedServerUnreachable(11),
                    dbBakFailedNetworkTimeout(12),
                    dbBakFailedFileCorruption(13),
                    dbBakFailedInternal(14),
                    dbResInProgress(15),
                    dbResCompleteSuccess(16),
                    dbResFailedTxInProgress(17),
                    dbResFailedServerUnreachable(18),
                    dbResFailedNetworkTimeout(19),
                    dbResFailedFileCorruption(20),
                    dbResFailedInternal(21)
                }
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "Indicates the operation status of the transfer."
    ::= { iShelfSysFileTransferEntry 8 }

-- %CLICOLLABEL OpDesc
-- %CLISHOWGROUP ftt.4
iShelfSysFileTransferOperStateDescription OBJECT-TYPE
    SYNTAX      IShelfGenericString
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "Provides variable meaning to the different operational states."
    ::= { iShelfSysFileTransferEntry 9 }

-- %CLICOLLABEL RowStatus
-- %CLISHOWGROUP ftt.1
-- %ROWSTATUSTYPE FIXED
iShelfSysFileTransferRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This object is used to modify the status of the
         associated row.  Settable objects within the row can only
         be modified when the row is in the notInService state.
         The notInService setting has no impact on the underlying
         Object but is used to cause atomic updates of the row
         once it is move to the active state.

         The supported values of RowStatus are:
            active (1)
            notInService (2)"
    ::= { iShelfSysFileTransferEntry 10 }
    

