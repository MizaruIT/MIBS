-- iShelfGetArpBindingsTable
--
-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++

-- %ROWSTRUCT USER
-- %CLICMDPREFIX data:ont
-- %CLICMDABBR arpbindingget
-- %CLISHOWGROUP arpbindingget
iShelfGetArpBindingsTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IShelfGetArpBindingsEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This table is used to initiate a query of the current ARP Binding IP address
         arp bindings for a specified ONT Ethernet port.
         
         Creation of a row in this table initiates the query. If a subsequent
         query for the same index is desired, that row must first be destroyed
         and then recreated. When a row in this table is destroyed, associated
         rows in the Arp Binding Table (contains the results of the query) will
         also be destroyed. Hence a row must not be destroyed unless a 
         subsequent query for the same index is desired.

         This table supports 10 row instances. It is possible to initiate queries
         for 10 ONT Ethernet ports at one time. The agent will delete the oldest
         row from the table if the max. # of entries is reached, and a new entry 
         is requested."
    ::= { iShelfDataOnt 11 }

iShelfGetArpBindingsEntry OBJECT-TYPE
    SYNTAX      IShelfGetArpBindingsEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "Creation of a row gets the ARP Binding information for the specified port."
    INDEX       { iShelfGetArpBindingsLocationType,
                  iShelfGetArpBindingsLocation,
                  iShelfGetArpBindingsSLCIndex,
                  iShelfGetArpBindingsPort }
    ::= { iShelfGetArpBindingsTable 1 }

IShelfGetArpBindingsEntry ::=
    SEQUENCE {
        iShelfGetArpBindingsLocationType
            IShelfLocationType,
        iShelfGetArpBindingsLocation
            IShelfLocation,
        iShelfGetArpBindingsSLCIndex
            IShelfOntSubscriberLineCardIndex,
        iShelfGetArpBindingsPort
            INTEGER,
        iShelfGetArpBindingsStatus
            INTEGER,
        iShelfGetArpBindingsRowStatus
            RowStatus
    }

-- %CLISHOWGROUP arpbindingget
iShelfGetArpBindingsLocationType OBJECT-TYPE
    SYNTAX      IShelfLocationType
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "The location type of the ARP Binding that is to be queried."
    ::= { iShelfGetArpBindingsEntry 1 }

-- %CLISHOWGROUP arpbindingget
iShelfGetArpBindingsLocation OBJECT-TYPE
    SYNTAX      IShelfLocation
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "The logical ID of the ONT that is to be queried
                 if the location type is logical."
    ::= { iShelfGetArpBindingsEntry 2 }

-- %CLISHOWGROUP arpbindingget
iShelfGetArpBindingsSLCIndex OBJECT-TYPE
    SYNTAX      IShelfOntSubscriberLineCardIndex
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "The subscriber line card on the ONT that is to be queried."
    ::= { iShelfGetArpBindingsEntry 3 }

-- %CLICOLLABEL Port
-- %CLIWIDTH 4
-- %CLISHOWGROUP arpbindingget
iShelfGetArpBindingsPort OBJECT-TYPE
    SYNTAX      INTEGER (1..4)
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "The Ethernet port on the ONT that is to be queried."
    ::= { iShelfGetArpBindingsEntry 4 }

-- %CLICOLLABEL Status
-- %CLISHOWGROUP arpbindingget
iShelfGetArpBindingsStatus OBJECT-TYPE
    SYNTAX      INTEGER {
                    unknown(0),
                    inProgress(1),
                    complete(2),
                    timeout(3)
                }
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "The status of this query operation."
    DEFVAL      { unknown }
    ::= { iShelfGetArpBindingsEntry 5 }

-- %ROWSTATUSTYPE RFC
-- %CLICOLLABEL RowStatus
-- %CLIWIDTH 12
-- %CLISHOWGROUP arpbindingget
iShelfGetArpBindingsRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This object is used for row creation, modification and
         deletion. Modification of objects within the row is only allowed
         when the row is not in the active state. In the case of an active
         row (already created), the row must be made notInService prior to
         modifying any settable objects. The notInService setting has no
         impact on the underlying object but is used to cause atomic updates
         of the row once it is moved to the active state.

         The supported values of RowStatus are:
            active (1)
            notInService (2)
            notReady (3)
            createAndGo (4)
            createAndWait (5)
            destroy (6)"
    ::= { iShelfGetArpBindingsEntry 6 }

