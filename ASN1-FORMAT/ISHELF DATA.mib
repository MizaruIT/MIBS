-- %SETINACTIVE
-- %CLIOPTION
iShelfOltBandwidthProfileLabel OBJECT-TYPE
    SYNTAX      IShelfLabel
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This object identifies the user description of the
         bandwidth profile.
         NOTE:  this object can only be modified with a non-active rowStatus."
    ::= { iShelfOltBandwidthProfileEntry 2 }

-- %CLISHOWGROUP bw.1
-- %SETINACTIVE
-- %CLICOLLABEL UsRate
-- %CLIOPTION
iShelfOltBandwidthProfileUsRate OBJECT-TYPE
    SYNTAX      IShelfOltPonBandwidthRate
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This object identifies the upstream bandwidth in
         Kbps.
         NOTE:  this object can only be modified with a non-active rowStatus."
    DEFVAL { 384 }
    ::= { iShelfOltBandwidthProfileEntry 3 }

-- %CLICOLLABEL DsMinRate
-- %CLISHOWGROUP bw.1
-- %SETINACTIVE
-- %CLIOPTION
iShelfOltBandwidthProfileDsMinimumRate OBJECT-TYPE
    SYNTAX      IShelfOltPonBandwidthRate
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This object identifies the minimum downstream
         bandwidth.  This value represents the guaranteed minimum
         downstream bandwidth.  The value is in Kbps.
         NOTE:  this object can only be modified with a non-active rowStatus."
    DEFVAL { 768 }
    ::= { iShelfOltBandwidthProfileEntry 4 }

-- %CLICOLLABEL DsMaxRate
-- %CLISHOWGROUP bw.1
-- %SETINACTIVE
-- %CLIOPTION
iShelfOltBandwidthProfileDsMaximumRate OBJECT-TYPE
    SYNTAX      IShelfOltPonBandwidthRate
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This object identifies the maximum downstream
         bandwidth.  This value represents the best effort maximum
         downstream bandwidth.  The subscriber can use data bandwidth up to the
         limit of their maximum bandwidth.
         If the sum of maximum bandwidth on a PON
         (for all ONTs) exceeds the total AAL5 bandwidth, the maximum
         bandwidth is shared amongst over-subscribers.  The value is in
         Kbps.
         NOTE:  this object can only be modified with a non-active rowStatus."
    DEFVAL { 768 }
    ::= { iShelfOltBandwidthProfileEntry 5 }

-- %ROWSTATUSTYPE RFC
-- %CLICOLLABEL RowStatus
-- %CLIWIDTH 12
-- %CLISHOWGROUP bw.2
iShelfOltBandwidthProfileRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This object is used for row creation, modification and
         deletion. Modification of objects within the row is only allowed
         when the row is not in the active state. In the case of an active
         row (already created), the row must be made notInService prior to
         modifying any settable objects. The notInService setting has no
         impact on the underlying object but is used to cause atomic updates
         of the row once it is moved to the active state.

         The supported values of RowStatus are:
            active (1)
            notInService (2)
            notReady (3)
            createAndGo (4)
            createAndWait (5)
            destroy (6)"
    ::= { iShelfOltBandwidthProfileEntry 6 }

