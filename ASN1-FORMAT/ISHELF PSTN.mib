-- %CLICMDPREFIX pstn:idlc
-- %CLICMDABBR idlctable
-- %CLISHOWGROUP idlctable
iShelfIdlcGroupTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IShelfIdlcGroupEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "This table is the definition for the GR303 IDLC Groups."
    ::= { iShelfLineAssoc 1 }

iShelfIdlcGroupEntry OBJECT-TYPE
    SYNTAX      IShelfIdlcGroupEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "Each row is a separate IDLC Group."
    INDEX       { iShelfIdlcGroupInterfaceId }
    ::= { iShelfIdlcGroupTable 1 }

IShelfIdlcGroupEntry ::=
    SEQUENCE {
        iShelfIdlcGroupInterfaceId
            INTEGER,
        iShelfIdlcGroupLabel
            IShelfLabel,
        iShelfIdlcGroupSwitchManufacturer
            IShelfIdlcSwitchManufacturer,
        iShelfIdlcGroupSecondaryDs1
            INTEGER,
        iShelfIdlcGroupInterfaceState
            IShelfIdlcInterfaceState,
        iShelfIdlcGroupActiveTmc
            IShelfIdlcActivePath,
        iShelfIdlcGroupPrimaryTmcState
            IShelfIdlcPathStatus,
        iShelfIdlcGroupSecondaryTmcState
            IShelfIdlcPathStatus,
        iShelfIdlcGroupActiveEoc
            IShelfIdlcActivePath,
        iShelfIdlcGroupPrimaryEocState
            IShelfIdlcPathStatus,
        iShelfIdlcGroupSecondaryEocState
            IShelfIdlcPathStatus,
        iShelfIdlcGroupRowStatus
            RowStatus
    }
