ISHELF-PSTN-MIB DEFINITIONS ::= BEGIN

IMPORTS
    OBJECT-TYPE,
    NOTIFICATION-TYPE,
    MODULE-IDENTITY
        FROM SNMPv2-SMI

    RowStatus
        FROM SNMPv2-TC

    telstrat,
    osiIShelf,
    iShelfPstn,
    iShelfModules
        FROM ISHELF-ARCS-MIB

    IShelfAdminState,
    IShelfCircuit,
    IShelfCircuitType,
    IShelfPermCCCircuitType,
    IShelfIdlcInterfaceState,
    IShelfIdlcActivePath,
    IShelfIdlcPathStatus,
    IShelfIdlcSwitchManufacturer,
    IShelfLabel,
    IShelfLocation,
    IShelfLocationType,
    IShelfOperState,
    IShelfOntLogicalId,
    IShelfOntSubscriberLineCardIndex,
    IShelfOntPotsCircuitAndDropString,
    IShelfOntPotsCircuitAndDropTests,
    IShelfCallStatus,
    IShelfServiceType
        FROM ISHELF-TC
    ;

iShelfPstnMIB MODULE-IDENTITY
    LAST-UPDATED "200604280000Z"
    ORGANIZATION "TelStrat"
    CONTACT-INFO "
        TelStrat
        6900 K Avenue
        Plano, TX, USA 75074

        Main Phone: 972-543-3500
        Main Fax: 972-424-2909
        Support Phone: 888-658-8572
        Support Fax: 918-610-9975
        Email:  AccessCustomerSupport@telstrat.com"
    DESCRIPTION
        "The MIB module for managing the PSTN Services on the
         IShelf System."
    REVISION "200604280000Z"
    DESCRIPTION "Beta Release"
    ::= { iShelfModules 6 }

-- %CLICMDPREFIX pstn:idlc
iShelfLineAssoc OBJECT IDENTIFIER ::= { iShelfPstn 1 }

