    SYNTAX      INTEGER (1..4)
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "The GR303 IDLC Group identifier."
    ::= { iShelfIdlcGroupEntry 1 }

-- %CLICOLLABEL Label
-- %CLISHOWGROUP idlctable.1
-- %SETINACTIVE
iShelfIdlcGroupLabel OBJECT-TYPE
    SYNTAX      IShelfLabel
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "A user description for the IDLC group.
         NOTE:  this object can only be modified with a non-active rowStatus."
    ::= { iShelfIdlcGroupEntry 2 }

-- %CLICOLLABEL Switch
-- %CLISHOWGROUP idlctable.1
-- %SETINACTIVE
iShelfIdlcGroupSwitchManufacturer OBJECT-TYPE
    SYNTAX      IShelfIdlcSwitchManufacturer
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "An identifier for the type of switch associated with this group.
         NOTE:  this object can only be modified with a non-active rowStatus."
    ::= { iShelfIdlcGroupEntry 3 }

-- %CLICOLLABEL SecDs1
-- %CLISHOWGROUP idlctable.1
-- %SETINACTIVE
-- %CLIWIDTH 6
iShelfIdlcGroupSecondaryDs1 OBJECT-TYPE
    SYNTAX      INTEGER (0..28)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This identifies which DS1 in the IDLC Facility table is designated as the
         secondary DS1 (contains the secondary TMC/EOC). Note that the primary DS1
         is always DS1 #1 in the IDLC Facility table.
         A value of zero means NONE. You cannot use a value of 1. Valid DS1 values
         are 2..28.
         NOTE:  this object can only be modified with a non-active rowStatus."
    DEFVAL { 0 }
    ::= { iShelfIdlcGroupEntry 4 }

