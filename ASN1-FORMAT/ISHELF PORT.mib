-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++

-- %DEEPGET osiOltOntPOTSTable 1000 ROW
-- %CLICMDPREFIX port:ont
-- %CLICMDABBR pots
-- %CLISHOWGROUP pots
iShelfOntPotsPortTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IShelfOntPotsPortEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "This table defines the ONT POTS port provisioning."
    ::= { iShelfPort 1 }

iShelfOntPotsPortEntry OBJECT-TYPE
    SYNTAX      IShelfOntPotsPortEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "Each row is a separate ONT POTS port."
    INDEX       { iShelfOntPotsPortLogicalId,
                  iShelfOntPotsPortSLCIndex,
                  iShelfOntPotsPortIndex }
    ::= { iShelfOntPotsPortTable 1 }

IShelfOntPotsPortEntry ::=
    SEQUENCE {
        iShelfOntPotsPortLogicalId
            IShelfOntLogicalId,
        iShelfOntPotsPortSLCIndex
            IShelfOntSubscriberLineCardIndex,
        iShelfOntPotsPortIndex
            INTEGER,
        iShelfOntPotsPortConserveAdminState
            IShelfAdminState,
        iShelfOntPotsPortHookState
            IShelfHookState,
        iShelfOntPotsPortImpedance
            Unsigned32,
        iShelfOntPotsPortRxGain
            INTEGER,
        iShelfOntPotsPortTxGain
            INTEGER,
        iShelfOntPotsPortLabel
            IShelfLabel,
        iShelfOntPotsPortAdminState
            IShelfAdminState,
        iShelfOntPotsPortOperState
            IShelfOperState,
        iShelfOntPotsPortLoopbackConfig
            IShelfLoopback,
        iShelfOntPotsPortLoopbackStatus
            IShelfLoopback,
        iShelfOntPotsPortTransmissionMode
            IShelfTransmissionMode,
        iShelfOntPotsPortSupervisoryMode
            IShelfSupervisoryMode,
        iShelfOntPotsPortRowStatus
            RowStatus
    }

-- %CLICOLLABEL LogicalId
-- %CLISHOWGROUP pots.1
-- %CLISHOWGROUP pots.2
-- %CLISHOWGROUP pots.3
iShelfOntPotsPortLogicalId OBJECT-TYPE
    SYNTAX      IShelfOntLogicalId
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "The logical ID of the ONT where the POTS port resides."
    ::= { iShelfOntPotsPortEntry 1 }

-- %CLICOLLABEL SlcIdx
-- %CLISHOWGROUP pots.1
iShelfOntPotsPortSLCIndex OBJECT-TYPE
    SYNTAX      IShelfOntSubscriberLineCardIndex
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "The line card on the ONT containing the POTS port."
    ::= { iShelfOntPotsPortEntry 2 }

-- %CLICOLLABEL Port
-- %CLISHOWGROUP pots.1
-- %CLIWIDTH 3
iShelfOntPotsPortIndex OBJECT-TYPE
    SYNTAX      INTEGER (1..8)
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "The POTS port."
    ::= { iShelfOntPotsPortEntry 3 }

-- %CLICOLLABEL ConsAdmin
-- %CLISHOWGROUP pots.1
-- %SETINACTIVE
iShelfOntPotsPortConserveAdminState OBJECT-TYPE
    SYNTAX      IShelfAdminState
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "The conserve admin state is the administrative state of the port when the
         ONT is operating on battery backup power. When operating on battery, locking
         the state means battery conservation is active and the port will be shut down.
         Unlocking the state means battery power is not conserved. The unlocked state
         is used for a critical service that must remain operational if battery backup
         power is active."
    DEFVAL      { unlock }
    ::= { iShelfOntPotsPortEntry 4 }

-- %DEEPOBJ osiOltOntPOTSHookState
-- %CLICOLLABEL Hook
-- %CLISHOWGROUP pots.1
iShelfOntPotsPortHookState OBJECT-TYPE
    SYNTAX      IShelfHookState
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "The current hook state, or telephone receiver position, of the POTS port."
    ::= { iShelfOntPotsPortEntry 5 }

-- %CLICOLLABEL Imp
