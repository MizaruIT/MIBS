    SYNTAX      INTEGER
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "The current POTS port receiver gain setting."
    ::= { iShelfOntPotsPortEntry 7 }

-- %CLICOLLABEL TxGain
-- %CLISHOWGROUP pots.1
-- %DEEPOBJ osiOltOntPOTSTxGain
iShelfOntPotsPortTxGain OBJECT-TYPE
    SYNTAX      INTEGER
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "The current POTS port transmitter gain setting."
    ::= { iShelfOntPotsPortEntry 8 }

-- %CLICOLLABEL Label
-- %CLISHOWGROUP pots.2
-- %SETINACTIVE
iShelfOntPotsPortLabel OBJECT-TYPE
    SYNTAX      IShelfLabel
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION "This is the user description of the port."
    ::= { iShelfOntPotsPortEntry 9 }

-- %CLICOLLABEL Admin
-- %CLISHOWGROUP pots.2
-- %SETINACTIVE
iShelfOntPotsPortAdminState OBJECT-TYPE
    SYNTAX      IShelfAdminState
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION "This indicates the desired administrative state of the port."
    DEFVAL      { unlock }
    ::= { iShelfOntPotsPortEntry 10 }

-- %DEEPOBJ osiOltOntPOTSOperState
-- %CLICOLLABEL Oper
-- %CLISHOWGROUP pots.2
iShelfOntPotsPortOperState OBJECT-TYPE
    SYNTAX      IShelfOperState
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "This indicates the current operational status of the port."
    ::= { iShelfOntPotsPortEntry 11 }

-- %CLICOLLABEL LoopbackCfg
-- %CLISHOWGROUP pots.3
-- %SETINACTIVE
iShelfOntPotsPortLoopbackConfig OBJECT-TYPE
    SYNTAX      IShelfLoopback
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION "This indicates the desired loopback state of the port."
    DEFVAL      { none }
    ::= { iShelfOntPotsPortEntry 12 }

-- %CLICOLLABEL Loopback
-- %CLISHOWGROUP pots.3
iShelfOntPotsPortLoopbackStatus OBJECT-TYPE
    SYNTAX      IShelfLoopback
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "This indicates the current loopback state of the port."
    ::= { iShelfOntPotsPortEntry 13 }

-- %CLICOLLABEL XMode
-- %CLISHOWGROUP pots.3
-- %SETINACTIVE
iShelfOntPotsPortTransmissionMode OBJECT-TYPE
    SYNTAX      IShelfTransmissionMode
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This object identifies the on-hook transmission (OHT) mode.
         NOTE:  this object can only be modified with a non-active rowStatus."
    DEFVAL      { partTimeOht }
    ::= { iShelfOntPotsPortEntry 14 }
    
-- %CLICOLLABEL SupMode
-- %CLISHOWGROUP pots.3
-- %SETINACTIVE
iShelfOntPotsPortSupervisoryMode OBJECT-TYPE
    SYNTAX      IShelfSupervisoryMode
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This object identifies the supervisory signaling mode for the line.
         NOTE:  this object can only be modified with a non-active rowStatus."
    DEFVAL      { loopStart }
    ::= { iShelfOntPotsPortEntry 15 }

-- %ROWSTATUSTYPE RFC
-- %CLICOLLABEL RowStatus
-- %CLIWIDTH 12
iShelfOntPotsPortRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This object is used for row creation, modification and
         deletion. Modification of objects within the row is only allowed
         when the row is not in the active state. In the case of an active
         row (already created), the row must be made notInService prior to
         modifying any settable objects. The notInService setting has no
         impact on the underlying object but is used to cause atomic updates
         of the row once it is moved to the active state.

         The supported values of RowStatus are:
            active (1)
            notInService (2)
            notReady (3)
            createAndGo (4)
            createAndWait (5)
            destroy (6)"
    ::= { iShelfOntPotsPortEntry 16 }

