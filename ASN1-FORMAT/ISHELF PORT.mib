    SEQUENCE {
        iShelfOntEthernetLocationType
            IShelfLocationType,
        iShelfOntEthernetLocation
            IShelfLocation,
        iShelfOntEthernetSLCIndex
            IShelfOntSubscriberLineCardIndex,
        iShelfOntEthernetPort
            INTEGER,
        iShelfOntEthernetConserveAdminState
            IShelfAdminState,
        iShelfOntEthernetAdminMode
            IShelfEthernetPortMode,
        iShelfOntEthernetOperMode
            IShelfEthernetPortMode,
        iShelfOntEthernetBandwidthProfilePointer
            IShelfOltBandwidthPointer,
        iShelfOntEthernetTrafficMode
            IShelfEthernetTrafficMode,
        iShelfOntEthernetMacAddress
            MacAddress,
        iShelfOntEthernetLabel
            IShelfLabel,
        iShelfOntEthernetAdminState
