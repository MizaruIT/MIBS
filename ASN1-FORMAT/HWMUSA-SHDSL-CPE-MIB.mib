	HWMUSA-SHDSL-CPE-MIB DEFINITIONS ::= BEGIN
 
		IMPORTS
			hwProducts			
				FROM HUAWEI-MIB			
			ifIndex			
				FROM IF-MIB			
			DisplayString			
				FROM RFC1213-MIB			
			IpAddress, Unsigned32, Gauge32, OBJECT-TYPE			
				FROM SNMPv2-SMI			
			TruthValue			
				FROM SNMPv2-TC;
	
	

		musa OBJECT IDENTIFIER ::= { hwProducts 6 }
		
		ias OBJECT IDENTIFIER ::= { musa 7 }
		
		hwIasService OBJECT IDENTIFIER ::= { ias 4 }
		
		hwMusaShdsl OBJECT IDENTIFIER ::= { hwIasService 4 }
		
		hwShdslCPE OBJECT IDENTIFIER ::= { hwMusaShdsl 2 }
		
		hwShdslCPECommon OBJECT IDENTIFIER ::= { hwShdslCPE 1 }
		
		umCommonInventoryTable OBJECT-TYPE
			SYNTAX SEQUENCE OF UmCommonInventoryEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"A table containing common inventory information."
			::= { hwShdslCPECommon 1 }
		
		umCommonInventoryEntry OBJECT-TYPE
			SYNTAX UmCommonInventoryEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"An entry in umCommonInventoryTable."
			INDEX { ifIndex }
			::= { umCommonInventoryTable 1 }
		
		UmCommonInventoryEntry ::=
			SEQUENCE { 
				umCommonInventoryModel
					INTEGER,
				umCommonInventorySwVer
					DisplayString,
				umCommonInventoryHwVer
					DisplayString,
				umCommonInventoryDevName
					DisplayString
			 }

		umCommonInventoryModel OBJECT-TYPE
			SYNTAX INTEGER
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				" This variable indicates the model number. "
			::= { umCommonInventoryEntry 1 }
		
		umCommonInventorySwVer OBJECT-TYPE
			SYNTAX DisplayString (SIZE (5))
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				" This variable indicates the software version.
				format: a.bcd
				(a, b, c, d: 0 ~ 9)
				"
			::= { umCommonInventoryEntry 2 }
		
		umCommonInventoryHwVer OBJECT-TYPE
			SYNTAX DisplayString (SIZE (5))
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				" This variable indicates the hardware version.
				format: a.bcd
				(a, b, c, d: 0 ~ 9)
				"
			::= { umCommonInventoryEntry 3 }
		
		umCommonInventoryDevName OBJECT-TYPE
			SYNTAX DisplayString (SIZE (8))
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"<write-operation is not provided for remote-side>
				This variable indicates the device name. "
			::= { umCommonInventoryEntry 4 }
		
		umCommonConfigTable OBJECT-TYPE
			SYNTAX SEQUENCE OF UmCommonConfigEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"A table containing common config information."
			::= { hwShdslCPECommon 2 }
		
		umCommonConfigEntry OBJECT-TYPE
			SYNTAX UmCommonConfigEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"An entry in umCommonConfigTable."
			INDEX { ifIndex }
			::= { umCommonConfigTable 1 }
		
		UmCommonConfigEntry ::=
			SEQUENCE { 
				umCommonConfigDate
					DisplayString,
				umCommonConfigTime
					DisplayString
			 }

		umCommonConfigDate OBJECT-TYPE
			SYNTAX DisplayString (SIZE (10))
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				" This variable is used to define date information of the device.
				The format is MM/DD/YYYY. MM means month, DD is day, and YYYY stands for year.
				The range is from 01/01/1990 to 12/31/2100.
				"
			::= { umCommonConfigEntry 1 }
		
		umCommonConfigTime OBJECT-TYPE
			SYNTAX DisplayString (SIZE (8))
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				" This variable is used to define time information of the device.
				The format is HH:MM:SS. HH means hour, MM is to indicate minute and SS is for second.
				"
			::= { umCommonConfigEntry 2 }
		
		umCommonActionTable OBJECT-TYPE
			SYNTAX SEQUENCE OF UmCommonActionEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"A table containing actions to CPE."
			::= { hwShdslCPECommon 3 }
		
		umCommonActionEntry OBJECT-TYPE
			SYNTAX UmCommonActionEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"An entry in umCommonActionTable."
			INDEX { ifIndex }
			::= { umCommonActionTable 1 }
		
		UmCommonActionEntry ::=
			SEQUENCE { 
				umCommonActionDbSave
					INTEGER,
				umCommonActionWarmStart
					INTEGER
			 }

		umCommonActionDbSave OBJECT-TYPE
			SYNTAX INTEGER { save(1) }
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				" This variable is to initiate the VCDB command.
				1: save
				"
			::= { umCommonActionEntry 1 }
		
		umCommonActionWarmStart OBJECT-TYPE
			SYNTAX INTEGER { restart(1) }
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				" This value is to restart the device.
				TRUE (1): warm start
				"
			::= { umCommonActionEntry 2 }
		
		umCommonSnmpConfigTable OBJECT-TYPE
			SYNTAX SEQUENCE OF UmCommonSnmpConfigEntry
			MAX-ACCESS not-accessible
			STATUS deprecated
			DESCRIPTION
				"A table containing SNMP configuration information."
			::= { hwShdslCPECommon 4 }
		
		umCommonSnmpConfigEntry OBJECT-TYPE
			SYNTAX UmCommonSnmpConfigEntry
			MAX-ACCESS not-accessible
			STATUS deprecated
			DESCRIPTION
				"An entry in umCommonSnmpConfigTable."
			INDEX { ifIndex }
			::= { umCommonSnmpConfigTable 1 }
		
		UmCommonSnmpConfigEntry ::=
			SEQUENCE { 
				umCommonSnmpConfigIpAddress
					IpAddress,
				umCommonSnmpConfigIpSubnetMask
					IpAddress,
				umCommonSnmpConfigIpGatewayIp
					IpAddress,
				umCommonSnmpConfigTrapIpAddress1
					IpAddress,
				umCommonSnmpConfigTrapIpPortNum1
					INTEGER,
				umCommonSnmpConfigTrapIpAddress2
					IpAddress,
				umCommonSnmpConfigTrapIpPortNum2
					INTEGER
			 }

		umCommonSnmpConfigIpAddress OBJECT-TYPE
			SYNTAX IpAddress
			MAX-ACCESS read-only
			STATUS deprecated
			DESCRIPTION
				" This variable is to indicate IP address of the device. "
			::= { umCommonSnmpConfigEntry 1 }
		
		umCommonSnmpConfigIpSubnetMask OBJECT-TYPE
			SYNTAX IpAddress
			MAX-ACCESS read-only
			STATUS deprecated
			DESCRIPTION
				" This variable is to define the subnet mask of the IP address of the device. "
			::= { umCommonSnmpConfigEntry 2 }
		
		umCommonSnmpConfigIpGatewayIp OBJECT-TYPE
			SYNTAX IpAddress
			MAX-ACCESS read-only
			STATUS deprecated
			DESCRIPTION
				" This variable is to define the gateway IP address of the device. "
			::= { umCommonSnmpConfigEntry 3 }
		
		umCommonSnmpConfigTrapIpAddress1 OBJECT-TYPE
			SYNTAX IpAddress
			MAX-ACCESS read-only
			STATUS deprecated
			DESCRIPTION
				" This variable is to define the IP address where the manager receives trap. "
			::= { umCommonSnmpConfigEntry 4 }
		
		umCommonSnmpConfigTrapIpPortNum1 OBJECT-TYPE
			SYNTAX INTEGER
			MAX-ACCESS read-only
			STATUS deprecated
			DESCRIPTION
				" This variable is to define the UDP port number where the manager receives trap.
				The possible values are:
				 162 (default)
				 62000
				 62001
				 62002
				"
			::= { umCommonSnmpConfigEntry 5 }
		
		umCommonSnmpConfigTrapIpAddress2 OBJECT-TYPE
			SYNTAX IpAddress
			MAX-ACCESS read-only
			STATUS deprecated
			DESCRIPTION
				" This variable is to define the IP address where the manager receives trap. "
			::= { umCommonSnmpConfigEntry 6 }
		
		umCommonSnmpConfigTrapIpPortNum2 OBJECT-TYPE
			SYNTAX INTEGER
			MAX-ACCESS read-only
			STATUS deprecated
			DESCRIPTION
				" This variable is to define the UDP port number where the manager receives trap.
				The possible values are:
				 162 (default)
				 62000
				 62001
				 62002
				"
			::= { umCommonSnmpConfigEntry 7 }
		
		hwShdslCPESdsl OBJECT IDENTIFIER ::= { hwShdslCPE 2 }
		
		umSdslConfigTable OBJECT-TYPE
			SYNTAX SEQUENCE OF UmSdslConfigEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"A table containing Sdsl configuration information."
			::= { hwShdslCPESdsl 1 }
		
		umSdslConfigEntry OBJECT-TYPE
			SYNTAX UmSdslConfigEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"An entry in umSdslConfigTable."
			INDEX { ifIndex }
			::= { umSdslConfigTable 1 }
		
		UmSdslConfigEntry ::=
			SEQUENCE { 
				umSdslConfigNEType
					INTEGER,
				umSdslConfigRateAdaptive
					INTEGER,
				umSdslConfigDataRate
					INTEGER,
				umSdslResetPerformance
					INTEGER,
				umSdslTimeElapsed
					INTEGER,
				umSdslValidIntervals
					INTEGER
			 }

		umSdslConfigNEType OBJECT-TYPE
			SYNTAX INTEGER
			MAX-ACCESS read-only
			STATUS deprecated
			DESCRIPTION
				" This variable is to set this STU-C or STU-R configuration.
				(0): STU-C (LT mode)
				(1): STU-R (NT mode)
				"
			::= { umSdslConfigEntry 1 }
		
		umSdslConfigRateAdaptive OBJECT-TYPE
			SYNTAX INTEGER
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				" This variable is to enable/disable the rate adaptive configuration.
				(0): Fixed
				(1): Adaptive
				"
			::= { umSdslConfigEntry 2 }
		
		umSdslConfigDataRate OBJECT-TYPE
			SYNTAX INTEGER (3..32)
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				" This variable is to set data rate (N*64k) of Sdsl. 
				If Rate Adaptive is TRUE, the value is invalid
				till Rate Adaptive is FALSE.
				"
			::= { umSdslConfigEntry 3 }
		
		umSdslResetPerformance OBJECT-TYPE
			SYNTAX INTEGER
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				" This value is to reset performance register. 
				1: reset all current performance data
				2: reset current ES
				3: reset current SES
				4: reset current UAS
				5: reset all history performance data
				6: reset history ES
				7: reset history SES
				8: reset history UAS
				"
			::= { umSdslConfigEntry 4 }
		
		umSdslTimeElapsed OBJECT-TYPE
			SYNTAX INTEGER (0..899)
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				" The number of seconds that have elapsed since
				the  beginning of the current error-measurement
				period."
			::= { umSdslConfigEntry 5 }
		
		umSdslValidIntervals OBJECT-TYPE
			SYNTAX INTEGER (0..96)
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				" The number of previous intervals for which
				valid data was collected. The value will be 96
				unless the interface was brought on-line within
				the last 24 hours, in which case the value will
				be the number of complete 15 minute intervals
				the since interface has been online. "
			::= { umSdslConfigEntry 6 }
		
		umSdslThresholdTable OBJECT-TYPE
			SYNTAX SEQUENCE OF UmSdslThresholdEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"A table containing MSDSL configuration information."
			::= { hwShdslCPESdsl 2 }
		
		umSdslThresholdEntry OBJECT-TYPE
			SYNTAX UmSdslThresholdEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"An entry in umSdslThresholdTable."
			INDEX { ifIndex }
			::= { umSdslThresholdTable 1 }
		
		UmSdslThresholdEntry ::=
			SEQUENCE { 
				umSdslThreshold15mES
					Gauge32,
				umSdslThreshold15mSES
					Gauge32,
				umSdslThreshold15mUAS
					Gauge32,
				umSdslThreshold24hES
					Gauge32,
				umSdslThreshold24hSES
					Gauge32,
				umSdslThreshold24hUAS
					Gauge32,
				umSdslThresholdSNRMargin
					Gauge32,
				umSdslThresholdLoopAttenuation
					Gauge32
			 }

		umSdslThreshold15mES OBJECT-TYPE
			SYNTAX Gauge32
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"This variable defines the 15-minute threshold of ES. "
			::= { umSdslThresholdEntry 1 }
		
		umSdslThreshold15mSES OBJECT-TYPE
			SYNTAX Gauge32
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"This variable defines the 15-minute threshold of SES. "
			::= { umSdslThresholdEntry 2 }
		
		umSdslThreshold15mUAS OBJECT-TYPE
			SYNTAX Gauge32
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"This variable defines the 15-minute threshold of UAS. "
			::= { umSdslThresholdEntry 3 }
		
		umSdslThreshold24hES OBJECT-TYPE
			SYNTAX Gauge32
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"This variable defines the 24-hour threshold of ES. "
			::= { umSdslThresholdEntry 4 }
		
		umSdslThreshold24hSES OBJECT-TYPE
			SYNTAX Gauge32
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"This variable defines the 24-hour threshold of SES. "
			::= { umSdslThresholdEntry 5 }
		
		umSdslThreshold24hUAS OBJECT-TYPE
			SYNTAX Gauge32
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"This variable defines the 24-hour threshold of UAS. "
			::= { umSdslThresholdEntry 6 }
		
		umSdslThresholdSNRMargin OBJECT-TYPE
			SYNTAX Gauge32
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				" This variable indicates the SNR margin threshold of the Sdsl line. 
				1~15: valid value
				"
			::= { umSdslThresholdEntry 7 }
		
		umSdslThresholdLoopAttenuation OBJECT-TYPE
			SYNTAX Gauge32
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				" This variable indicates the Loop Attenuation threshold of the Sdsl line. 
				1~127: 
				"
			::= { umSdslThresholdEntry 8 }
		
		umSdslPmCurrentTable OBJECT-TYPE
			SYNTAX SEQUENCE OF UmSdslPmCurrentEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"The Sdsl PM Current table."
			::= { hwShdslCPESdsl 3 }
		
		umSdslPmCurrentEntry OBJECT-TYPE
			SYNTAX UmSdslPmCurrentEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"An entry in the Sdsl Pm Current table."
			INDEX { ifIndex }
			::= { umSdslPmCurrentTable 1 }
		
		UmSdslPmCurrentEntry ::=
			SEQUENCE { 
				umSdslPmCurrentES
					Gauge32,
				umSdslPmCurrentSES
					Gauge32,
				umSdslPmCurrentUAS
					Gauge32
			 }

		umSdslPmCurrentES OBJECT-TYPE
			SYNTAX Gauge32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The number of Errored Seconds encountered by an Sdsl
				during the current interval."
			::= { umSdslPmCurrentEntry 1 }
		
		umSdslPmCurrentSES OBJECT-TYPE
			SYNTAX Gauge32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The number of Severely Errored Seconds encountered by an Sdsl
				during the current interval."
			::= { umSdslPmCurrentEntry 2 }
		
		umSdslPmCurrentUAS OBJECT-TYPE
			SYNTAX Gauge32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The number of Unavailable Seconds encountered by an Sdsl
				during the current interval."
			::= { umSdslPmCurrentEntry 3 }
		
		umSdslPmIntervalTable OBJECT-TYPE
			SYNTAX SEQUENCE OF UmSdslPmIntervalEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"The Sdsl Interval table."
			::= { hwShdslCPESdsl 4 }
		
		umSdslPmIntervalEntry OBJECT-TYPE
			SYNTAX UmSdslPmIntervalEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"An entry in the Sdsl Interval table."
			INDEX { ifIndex, umSdslPmIntervalIndex }
			::= { umSdslPmIntervalTable 1 }
		
		UmSdslPmIntervalEntry ::=
			SEQUENCE { 
				umSdslPmIntervalIndex
					INTEGER,
				umSdslPmIntervalES
					Gauge32,
				umSdslPmIntervalSES
					Gauge32,
				umSdslPmIntervalUAS
					Gauge32
			 }

		umSdslPmIntervalIndex OBJECT-TYPE
			SYNTAX INTEGER (1..96)
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"A number between 1 and 96, where 1 is the most recently completed 15 minute interval and
				96 is the least recently completed 15 minutes interval. "
			::= { umSdslPmIntervalEntry 1 }
		
		umSdslPmIntervalES OBJECT-TYPE
			SYNTAX Gauge32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The number of Errored Seconds encountered by an Sdsl in one interval."
			::= { umSdslPmIntervalEntry 2 }
		
		umSdslPmIntervalSES OBJECT-TYPE
			SYNTAX Gauge32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The number of Severely Errored Seconds encountered by an Sdsl in one interval."
			::= { umSdslPmIntervalEntry 3 }
		
		umSdslPmIntervalUAS OBJECT-TYPE
			SYNTAX Gauge32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The number of Unavailable Seconds encountered by an Sdsl in one interval."
			::= { umSdslPmIntervalEntry 4 }
		
		umSdslPmCurrentDayTable OBJECT-TYPE
			SYNTAX SEQUENCE OF UmSdslPmCurrentDayEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"The Sdsl PM Current Day table. (0:00AM ~ the last completed quarter)"
			::= { hwShdslCPESdsl 5 }
		
		umSdslPmCurrentDayEntry OBJECT-TYPE
			SYNTAX UmSdslPmCurrentDayEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"An entry in the Sdsl Pm Current Day table."
			INDEX { ifIndex }
			::= { umSdslPmCurrentDayTable 1 }
		
		UmSdslPmCurrentDayEntry ::=
			SEQUENCE { 
				umSdslPmCurrentDayES
					Gauge32,
				umSdslPmCurrentDaySES
					Gauge32,
				umSdslPmCurrentDayUAS
					Gauge32
			 }

		umSdslPmCurrentDayES OBJECT-TYPE
			SYNTAX Gauge32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The number of Errored Seconds encountered by an Sdsl
				during the Current Day."
			::= { umSdslPmCurrentDayEntry 1 }
		
		umSdslPmCurrentDaySES OBJECT-TYPE
			SYNTAX Gauge32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The number of Severely Errored Seconds encountered by an Sdsl
				during the Current Day."
			::= { umSdslPmCurrentDayEntry 2 }
		
		umSdslPmCurrentDayUAS OBJECT-TYPE
			SYNTAX Gauge32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The number of Unavailable Seconds encountered by an Sdsl
				during the Current Day."
			::= { umSdslPmCurrentDayEntry 3 }
		
		umSdslPmDayTable OBJECT-TYPE
			SYNTAX SEQUENCE OF UmSdslPmDayEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"The SDSL Pm Day table."
			::= { hwShdslCPESdsl 6 }
		
		umSdslPmDayEntry OBJECT-TYPE
			SYNTAX UmSdslPmDayEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"An entry in the Sdsl Pm Day table."
			INDEX { ifIndex, umSdslPmDayIndex }
			::= { umSdslPmDayTable 1 }
		
		UmSdslPmDayEntry ::=
			SEQUENCE { 
				umSdslPmDayIndex
					INTEGER,
				umSdslPmDayES
					Gauge32,
				umSdslPmDaySES
					Gauge32,
				umSdslPmDayUAS
					Gauge32
			 }

		umSdslPmDayIndex OBJECT-TYPE
			SYNTAX INTEGER (1..7)
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"A number between 1 and 7, where 1 is the most recently completed day and
				7 is the least recently completed day. "
			::= { umSdslPmDayEntry 1 }
		
		umSdslPmDayES OBJECT-TYPE
			SYNTAX Gauge32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The number of Errored Seconds encountered by an Sdsl in one day. "
			::= { umSdslPmDayEntry 2 }
		
		umSdslPmDaySES OBJECT-TYPE
			SYNTAX Gauge32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The number of Severely Errored Seconds encountered by an Sdsl in one day."
			::= { umSdslPmDayEntry 3 }
		
		umSdslPmDayUAS OBJECT-TYPE
			SYNTAX Gauge32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The number of Unavailable Seconds  encountered by an Sdsl in one day."
			::= { umSdslPmDayEntry 4 }
		
		umSdslTestTable OBJECT-TYPE
			SYNTAX SEQUENCE OF UmSdslTestEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"A table containing SHDSL testing function."
			::= { hwShdslCPESdsl 7 }
		
		umSdslTestEntry OBJECT-TYPE
			SYNTAX UmSdslTestEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"An entry in umSdslTestTable."
			INDEX { ifIndex }
			::= { umSdslTestTable 1 }
		
		UmSdslTestEntry ::=
			SEQUENCE { 
				umSdslTestLoopback
					INTEGER,
				umSdslTestPattern
					INTEGER,
				umSdslTestPeriod
					INTEGER,
				umSdslTestStart
					TruthValue,
				umSdslTestInsertOneBitError
					TruthValue,
				umSdslTestElapsedTime
					INTEGER,
				umSdslTestErrCount
					DisplayString,
				umSdslTestErrRate
					DisplayString
			 }

		umSdslTestLoopback OBJECT-TYPE
			SYNTAX INTEGER
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				" This value is to set loopback.
				0: Release       
				2: Loopback toward Network  
				"
			::= { umSdslTestEntry 1 }
		
		umSdslTestPattern OBJECT-TYPE
			SYNTAX INTEGER
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				" This variable is used to set test pattern.
				1: 511
				2: 2047
				3: PRBS
				Support Eoc Version 1.02 (23/04/2002)
				"
			::= { umSdslTestEntry 2 }
		
		umSdslTestPeriod OBJECT-TYPE
			SYNTAX INTEGER
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				" This variable is used to get/set test period in second.
				A value of 0 means non-stop testing.
				"
			::= { umSdslTestEntry 3 }
		
		umSdslTestStart OBJECT-TYPE
			SYNTAX TruthValue
