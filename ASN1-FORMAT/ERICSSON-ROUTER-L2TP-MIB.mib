eriRouterL2tpPeerTunCfgTable  OBJECT-TYPE
        SYNTAX          SEQUENCE OF EriRouterL2tpPeerTunCfgEntry
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION
           "This table contains objects which identify L2TP peers and tunnels."
        ::= { eriRouterL2tpMibObjects 8 }

eriRouterL2tpPeerTunCfgEntry  OBJECT-TYPE
        SYNTAX          EriRouterL2tpPeerTunCfgEntry
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION
            "A conceptual row in the eriRouterL2tpPeerTunCfgTable.  An entry in
            this table corresponds to a single tunnel of a L2TP peer.
            Entries are added to this table as tunnels become connected.
            Entries are removed from the table if the communication with one
            or both ends of a tunnel is lost."
        INDEX { eriRouterL2tpMibPeerName, eriRouterL2tpMibTunnelID }
        ::= { eriRouterL2tpPeerTunCfgTable 1 }

EriRouterL2tpPeerTunCfgEntry ::=
        SEQUENCE {
	    eriRouterL2tpPTCfgTunnelRemoteID         Unsigned32,
	    eriRouterL2tpPTCfgTunnelLocalNm          SnmpAdminString,
	    eriRouterL2tpPTCfgTunnelRemoteNm         SnmpAdminString,
	    eriRouterL2tpPTCfgPeerLocalNm            SnmpAdminString,
	    eriRouterL2tpPTCfgTunnelRemoteIPAddrType InetAddressType,
	    eriRouterL2tpPTCfgTunnelRemoteIPAddr     InetAddress,
	    eriRouterL2tpPTCfgTunnelState            TunStateType,
	    eriRouterL2tpPTCfgTunnelContext          SnmpAdminString,
	    eriRouterL2tpPTCfgSessionCount           Gauge32,
	    eriRouterL2tpPTCfgActSessCnt             Gauge32,
	    eriRouterL2tpPTCfgTotActSessions         Counter32,
	    eriRouterL2tpPTCfgTotFailSessions        Counter32
        }

eriRouterL2tpPTCfgTunnelRemoteID OBJECT-TYPE
        SYNTAX          Unsigned32 (0..65535)
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
            "This object indicates the remote tunnel ID.  A value of zero
            indicates that the remote tunnel ID could not be determined."
        ::= { eriRouterL2tpPeerTunCfgEntry 1 }

eriRouterL2tpPTCfgTunnelLocalNm OBJECT-TYPE
        SYNTAX  	SnmpAdminString (SIZE (0..80))
        MAX-ACCESS  	read-only
        STATUS      	current
        DESCRIPTION
            "This object indicates the local name that was given to this
            tunnel.  The length of the string will be zero if no local
            name assigned to the tunnel."
        ::= { eriRouterL2tpPeerTunCfgEntry 2 }

eriRouterL2tpPTCfgTunnelRemoteNm OBJECT-TYPE
        SYNTAX  	SnmpAdminString (SIZE (1..80))
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
            "This object indicates the name that was given to the remote
            end of this tunnel.  The length of the string will be zero if
            no tunnel name can be obtained from the peer." 
        ::= { eriRouterL2tpPeerTunCfgEntry 3 }

eriRouterL2tpPTCfgPeerLocalNm OBJECT-TYPE
        SYNTAX          SnmpAdminString (SIZE (0..80))
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION 
            "This object identifies the local peer name.  The empty peer
            name (zero length) indicates that the name could not be
            determined."
        ::= { eriRouterL2tpPeerTunCfgEntry 4 }

eriRouterL2tpPTCfgTunnelRemoteIPAddrType OBJECT-TYPE
        SYNTAX          InetAddressType     
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
            "This object identifies the address type contained in 
            eriRouterL2tpPTCfgTunnelRemoteIPAddr."
        ::= { eriRouterL2tpPeerTunCfgEntry 5 }

eriRouterL2tpPTCfgTunnelRemoteIPAddr OBJECT-TYPE
    	SYNTAX          InetAddress
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
            "This object identifies the remote IP address for
            this tunnel (utilized only with User Datagram Protocol)."
        ::= { eriRouterL2tpPeerTunCfgEntry 6 }

eriRouterL2tpPTCfgTunnelState OBJECT-TYPE
        SYNTAX          TunStateType
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
            "This object is the current state of the tunnel."
        ::= { eriRouterL2tpPeerTunCfgEntry 7 }

eriRouterL2tpPTCfgTunnelContext OBJECT-TYPE
        SYNTAX          SnmpAdminString (SIZE (0..80))
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
            "This object identifies the context of this tunnel. The empty
            context name (zero length) indicates that the context could
            not be determined.  Any context name length greater than 80
            will be truncated."
        ::= { eriRouterL2tpPeerTunCfgEntry 8 }

eriRouterL2tpPTCfgSessionCount OBJECT-TYPE
        SYNTAX          Gauge32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION 
            "The number of sessions belonging to this tunnel."
        ::= { eriRouterL2tpPeerTunCfgEntry 9 }

