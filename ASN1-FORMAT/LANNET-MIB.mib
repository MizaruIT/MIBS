	  ro104RJ45(25),
      ro104D9(26),
          ltrf4Port(31),      -- NEW
      ltrf16Port(32),         -- NEW
          itlbRoPort(36),     -- 2.0.2
      itlbRiPort(37),         -- 2.0.2
