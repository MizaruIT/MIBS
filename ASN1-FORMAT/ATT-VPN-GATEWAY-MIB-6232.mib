        tunInitVpnBkupCon    INTEGER,
        tunConnectionId      DisplayString,
        tunName              DisplayString,
        tunDirection         INTEGER
    }

ngtuIndex OBJECT-TYPE
    SYNTAX     Integer32 (1..65535)
    MAX-ACCESS not-accessible
    STATUS     current
    DESCRIPTION
        "A unique value for each logical tunnel
        contained by the device."
