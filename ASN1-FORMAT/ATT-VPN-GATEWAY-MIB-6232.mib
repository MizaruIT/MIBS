attVpnGateway8200         OBJECT IDENTIFIER ::= { ngPlatforms 1}
attVpnGateway8300         OBJECT IDENTIFIER ::= { ngPlatforms 2}
attVpnGatewayVM           OBJECT IDENTIFIER ::= { ngPlatforms 3}

